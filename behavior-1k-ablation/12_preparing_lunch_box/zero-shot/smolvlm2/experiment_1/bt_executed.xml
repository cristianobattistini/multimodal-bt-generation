      <Action ID="NAVIGATE_TO" obj="half_apple_1" />
      <!-- Grasp half_apple_1 -->
      <Action ID="GRASP" obj="half_apple_1" />
      <!-- Place inside packing_box -->
      <Action ID="PLACE_INSIDE" obj="packing_box" />
      <!-- Open refrigerator -->
      <Action ID="OPEN" obj="refrigerator" />
