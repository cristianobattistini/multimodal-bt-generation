      <Action ID="NAVIGATE_TO" obj="club_sandwich" />
      <!-- Grasp club_sandwich -->
      <Action ID="GRASP" obj="club_sandwich" />
      <!-- Place inside packing_box -->
      <Action ID="PLACE_INSIDE" obj="packing_box" />
      <!-- Close refrigerator -->
      <Action ID="CLOSE" obj="refrigerator" />
    </Sequence>
  </BehaviorTree>
</root>