<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to sack -->
      <Action ID="NAVIGATE_TO" obj="sack" />
      <!-- Grasp sack -->
      <Action ID="GRASP" obj="sack" />
      <!-- Navigate to car -->
      <Action ID="NAVIGATE_TO" obj="car" />
      <!-- Place next to car -->
      <Action ID="PLACE_NEXT_TO" obj="car" />
      <!-- Navigate to electric_refrigerator -->
      <Action ID="NAVIGATE_TO" obj="electric_refrigerator" />
      <!-- Open electric_refrigerator -->
      <Action ID="OPEN" obj="electric_refrigerator" />
      <!-- Navigate to sack -->
      <Action ID="NAVIGATE_TO" obj="sack" />
      <!-- Grasp sack -->
      <Action ID="GRASP" obj="sack" />
      <!-- Go to electric_refrigerator -->
      <Action ID="NAVIGATE_TO" obj="electric_refrigerator" />
      <!-- Place inside electric_refrigerator -->
      <Action ID="PLACE_INSIDE" obj="electric_refrigerator" />
      <!-- Navigate to beefsteak_tomato -->
      <Action ID="NAVIGATE_TO" obj="beefsteak_tomato" />
      <!-- Grasp beefsteak_tomato -->
      <Action ID="GRASP" obj="beefsteak_tomato" />
      <!-- Navigate to carton_of_milk -->
      <Action ID="NAVIGATE_TO" obj="carton_of_milk" />
      <!-- Go to milk -->
      <Action ID="NAVIGATE_TO" obj="milk" />
      <!-- Grasp milk -->
      <Action ID="GRASP" obj="milk" />
      <!-- Go to electric_refrigerator -->
      <Action ID="NAVIGATE_TO" obj="electric_refrigerator" />
      <!-- Open electric_refrigerator -->
      <Action ID="OPEN" obj="electric_refrigerator" />
      <!-- Place inside electric_refrigerator -->
      <Action ID="PLACE_INSIDE" obj="electric_refrigerator" />
      <!-- Close electric_refrigerator -->
      <Action ID="CLOSE" obj="electric_refrigerator" />
      <!-- Navigate to car -->
      <Action ID="NAVIGATE_TO" obj="car" />
      <!-- Close car -->
      <Action ID="CLOSE" obj="car" />
    </Sequence>
  </BehaviorTree>
</root>