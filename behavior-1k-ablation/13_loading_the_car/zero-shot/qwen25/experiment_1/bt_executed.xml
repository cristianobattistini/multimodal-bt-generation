<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to car -->
      <Action ID="NAVIGATE_TO" obj="car" />
      <!-- Open car -->
      <Action ID="OPEN" obj="car" />
      <!-- Grasp tennis_racket -->
      <Action ID="GRASP" obj="tennis_racket" />
      <!-- Navigate to trunk -->
      <Action ID="NAVIGATE_TO" obj="trunk" />
      <!-- Place inside trunk -->
      <Action ID="PLACE_INSIDE" obj="trunk" />
      <!-- Navigate to digital_camera -->
      <Action ID="NAVIGATE_TO" obj="digital_camera" />
      <!-- Grasp digital_camera -->
      <Action ID="GRASP" obj="digital_camera" />
      <!-- Navigate to container -->
      <Action ID="NAVIGATE_TO" obj="container" />
      <!-- Place inside container -->
      <Action ID="PLACE_INSIDE" obj="container" />
      <!-- Navigate to container -->
      <Action ID="NAVIGATE_TO" obj="container" />
      <!-- Open container -->
      <Action ID="OPEN" obj="container" />
      <!-- Navigate to container -->
      <Action ID="NAVIGATE_TO" obj="container" />
      <!-- Grasp container -->
      <Action ID="GRASP" obj="container" />
      <!-- Navigate to trunk -->
      <Action ID="NAVIGATE_TO" obj="trunk" />
      <!-- Place inside trunk -->
      <Action ID="PLACE_INSIDE" obj="trunk" />
      <!-- Close trunk -->
      <Action ID="CLOSE" obj="trunk" />
    </Sequence>
  </BehaviorTree>
</root>