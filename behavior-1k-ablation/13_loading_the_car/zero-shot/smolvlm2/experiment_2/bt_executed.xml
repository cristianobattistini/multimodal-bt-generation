<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to car -->
      <Action ID="NAVIGATE_TO" obj="car" />
      <!-- Grasp car -->
      <Action ID="GRASP" obj="car" />
      <!-- Open car -->
      <Action ID="OPEN" obj="car" />
      <!-- Navigate to tennis_racket -->
      <Action ID="NAVIGATE_TO" obj="tennis_racket" />
      <!-- Place tennis_racket inside car -->
      <Action ID="PLACE_INSIDE" obj="tennis_racket" />
      <!-- Navigate to container -->
      <Action ID="NAVIGATE_TO" obj="container" />
      <!-- Open container -->
      <Action ID="OPEN" obj="container" />
      <!-- Navigate to digital_camera -->
      <Action ID="NAVIGATE_TO" obj="digital_camera" />
      <!-- Place digital_camera inside car -->
      <Action ID="PLACE_INSIDE" obj="digital_camera" />
      <!-- Navigate to garage -->
      <Action ID="NAVIGATE_TO" obj="garage" />
      <!-- Open garage -->
      <Action ID="OPEN" obj="garage" />
      <!-- Close car -->
      <Action ID="CLOSE" obj="car" />
    </Sequence>
  </BehaviorTree>
</root>