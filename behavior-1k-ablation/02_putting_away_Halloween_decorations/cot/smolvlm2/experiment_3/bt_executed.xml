<root main_tree_to_execute="MainTree">
  <Sequence>
    <!-- Open the cabinet -->
    <Action ID="OPEN" obj="cabinet" />
    <!-- Navigate to pumpkin -->
    <Action ID="NAVIGATE_TO" obj="pumpkin" />
    <!-- Grasp pumpkin -->
    <Action ID="GRASP" obj="pumpkin" />
    <!-- Place inside cabinet -->
    <Action ID="PLACE_INSIDE" obj="cabinet" />
    <!-- Place next to table -->
    <Action ID="PLACE_NEXT_TO" obj="table" />
  </Sequence>
  <!-- Close cabinet -->
  <Action ID="CLOSE" obj="cabinet" />
  <!-- Place cauldron next to table -->
  <Action ID="PLACE_NEXT_TO" obj="table" />
</root>