<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to table avoiding book_stack obstacle -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Navigate to blue_pen by moving around the book_stack into the pen holder -->
      <Action ID="NAVIGATE_TO" obj="blue_pen"/>
      <!-- Grasp blue_pen -->
      <Action ID="GRASP" obj="blue_pen"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>