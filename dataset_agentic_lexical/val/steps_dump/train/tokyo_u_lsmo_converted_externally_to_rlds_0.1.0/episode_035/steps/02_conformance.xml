<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pen_holder (move around book_stack obstacle) -->
      <Action ID="NAVIGATE_TO" obj="pen_holder"/>
      <!-- Navigate to blue_pen (reach into pen_holder to access the pen) -->
      <Action ID="NAVIGATE_TO" obj="blue_pen"/>
      <!-- Grasp blue_pen -->
      <Action ID="GRASP" obj="blue_pen"/>
    </Sequence>
  </BehaviorTree>
</root>