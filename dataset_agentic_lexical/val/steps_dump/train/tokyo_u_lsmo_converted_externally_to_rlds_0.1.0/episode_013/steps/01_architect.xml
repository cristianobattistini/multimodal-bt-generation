<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate around dark brown box to approach scissors -->
      <Action ID="NAVIGATE_TO" obj="dark_brown_box"/>
      <!-- Navigate to scissors -->
      <Action ID="NAVIGATE_TO" obj="scissors"/>
      <!-- Grasp scissors -->
      <Action ID="GRASP" obj="scissors"/>
    </Sequence>
  </BehaviorTree>
</root>