<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp" />
      <!-- Pick grid_clamp -->
      <Action ID="PICK" obj="grid_clamp" />
      <!-- Go to grid_board -->
      <Action ID="GO_TO" obj="grid_board" />
      <!-- Place grid_clamp on grid_board -->
      <Action ID="PLACE_ON_TOP" obj="grid_clamp" surface="grid_board" />
    </Sequence>
  </BehaviorTree>
</root>
