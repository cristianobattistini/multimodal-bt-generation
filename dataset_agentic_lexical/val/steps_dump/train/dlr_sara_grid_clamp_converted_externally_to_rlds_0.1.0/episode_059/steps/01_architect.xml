<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Locomote to grid_clamp -->
      <Action ID="LOCOMOTE_TO" obj="grid_clamp" />
      <!-- Grasp grid_clamp -->
      <Action ID="GRASP" obj="grid_clamp" />
      <!-- Move to table -->
      <Action ID="MOVE_TO" obj="table" />
      <!-- Place on table (grid area) -->
      <Action ID="PLACE_ON_TOP" obj="table" />
    </Sequence>
  </BehaviorTree>
</root>
