<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp"/>
      <!-- Grasp grid_clamp -->
      <Action ID="GRASP" obj="grid_clamp"/>
      <!-- Navigate to grid_surface -->
      <Action ID="NAVIGATE_TO" obj="grid_surface"/>
      <!-- Place on grid_surface -->
      <Action ID="PLACE_ON_TOP" obj="grid_surface"/>
    </Sequence>
  </BehaviorTree>
</root>