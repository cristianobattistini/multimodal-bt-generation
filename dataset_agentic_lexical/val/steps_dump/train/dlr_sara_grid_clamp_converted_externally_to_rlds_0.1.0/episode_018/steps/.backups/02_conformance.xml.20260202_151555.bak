<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp"/>
      <!-- Grasp grid_clamp -->
      <Action ID="GRASP" obj="grid_clamp"/>
      <!-- Navigate to grid -->
      <Action ID="NAVIGATE_TO" obj="grid"/>
      <!-- Place on grid (onto wooden grid near parallel rails) -->
      <Action ID="PLACE_ON_TOP" obj="grid"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>