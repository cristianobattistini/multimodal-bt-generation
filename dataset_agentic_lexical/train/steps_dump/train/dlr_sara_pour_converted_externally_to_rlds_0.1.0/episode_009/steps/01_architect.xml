<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to brown_cup -->
      <Action ID="NAVIGATE_TO" obj="brown_cup"/>
      <!-- Grasp brown_cup -->
      <Action ID="GRASP" obj="brown_cup"/>
      <!-- Navigate to white_mug -->
      <Action ID="NAVIGATE_TO" obj="white_mug"/>
      <!-- Pour into white_mug -->
      <Action ID="POUR" obj="white_mug"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>