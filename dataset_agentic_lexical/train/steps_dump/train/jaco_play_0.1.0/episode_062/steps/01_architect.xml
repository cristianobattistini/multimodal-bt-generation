<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to yellow_cup -->
      <Action ID="NAVIGATE_TO" obj="yellow_cup" />
      <!-- Grasp yellow_cup -->
      <Action ID="GRASP" obj="yellow_cup" />
      <!-- Move to dish_rack -->
      <Action ID="MOVE_TO" obj="dish_rack" />
      <!-- Place inside dish_rack -->
      <Action ID="PLACE_INSIDE" obj="dish_rack" />
    </Sequence>
  </BehaviorTree>
</root>
