<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to gray_bowl -->
      <Action ID="MOVE_TO" obj="gray_bowl" />
      <!-- Pick gray_bowl -->
      <Action ID="PICK" obj="gray_bowl" />
      <!-- Go to dish_rack -->
      <Action ID="GO_TO" obj="dish_rack" />
      <!-- Place gray_bowl inside dish_rack -->
      <Action ID="PLACE_INSIDE" obj="gray_bowl" container="dish_rack" />
    </Sequence>
  </BehaviorTree>
</root>
