<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_bowl -->
      <Action ID="NAVIGATE_TO" obj="blue_bowl"/>
      <!-- Navigate to butter_dairy -->
      <Action ID="NAVIGATE_TO" obj="butter_dairy"/>
      <!-- Grasp butter_dairy -->
      <Action ID="GRASP" obj="butter_dairy"/>
      <!-- Navigate to white_plate -->
      <Action ID="NAVIGATE_TO" obj="white_plate"/>
      <!-- Place on white_plate -->
      <Action ID="PLACE_ON_TOP" obj="white_plate"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>