<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_tray -->
      <Action ID="NAVIGATE_TO" obj="blue_tray"/>
      <!-- Navigate to apple (approach the apple in the blue_tray) -->
      <Action ID="NAVIGATE_TO" obj="apple"/>
      <!-- Grasp apple -->
      <Action ID="GRASP" obj="apple"/>
    </Sequence>
  </BehaviorTree>
</root>