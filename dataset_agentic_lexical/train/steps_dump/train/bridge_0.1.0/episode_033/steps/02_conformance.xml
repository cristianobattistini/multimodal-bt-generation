<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to cucumber -->
      <Action ID="NAVIGATE_TO" obj="cucumber"/>
      <!-- Grasp cucumber -->
      <Action ID="GRASP" obj="cucumber"/>
      <!-- Navigate to counter (upper-left area) -->
      <Action ID="NAVIGATE_TO" obj="counter"/>
      <!-- Place on counter -->
      <Action ID="PLACE_ON_TOP" obj="counter"/>
    </Sequence>
  </BehaviorTree>
</root>