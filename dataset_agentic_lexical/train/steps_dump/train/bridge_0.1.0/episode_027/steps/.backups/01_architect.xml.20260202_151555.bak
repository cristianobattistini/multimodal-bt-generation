<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to yellow_towel -->
      <Action ID="NAVIGATE_TO" obj="yellow_towel"/>
      <!-- Grasp yellow_towel -->
      <Action ID="GRASP" obj="yellow_towel"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table to the left of the colander -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>