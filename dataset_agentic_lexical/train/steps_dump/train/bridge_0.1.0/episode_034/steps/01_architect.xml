<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to red_pepper -->
      <Action ID="NAVIGATE_TO" obj="red_pepper"/>
      <!-- Grasp red_pepper -->
      <Action ID="GRASP" obj="red_pepper"/>
      <!-- Navigate to table (place on bottom-right corner of table) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>