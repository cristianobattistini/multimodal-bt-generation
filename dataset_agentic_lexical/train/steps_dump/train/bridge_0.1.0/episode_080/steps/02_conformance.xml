<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to carrot -->
      <Action ID="NAVIGATE_TO" obj="carrot" />
      <!-- Grasp carrot -->
      <Action ID="GRASP" obj="carrot" />
      <!-- Go to plate -->
      <Action ID="GO_TO" obj="plate" />
      <!-- Place carrot on plate -->
      <Action ID="PLACE_ON_TOP" obj="carrot" surface="plate" />
    </Sequence>
  </BehaviorTree>
</root>
