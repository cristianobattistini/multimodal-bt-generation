<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to sushi -->
      <Action ID="NAVIGATE_TO" obj="sushi"/>
      <!-- Grasp sushi -->
      <Action ID="GRASP" obj="sushi"/>
      <!-- Navigate to green_plate -->
      <Action ID="NAVIGATE_TO" obj="green_plate"/>
      <!-- Place on green_plate -->
      <Action ID="PLACE_ON_TOP" obj="green_plate"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>