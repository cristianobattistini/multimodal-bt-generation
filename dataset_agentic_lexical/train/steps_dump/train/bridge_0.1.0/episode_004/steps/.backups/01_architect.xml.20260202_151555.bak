<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to kadai -->
      <Action ID="NAVIGATE_TO" obj="kadai"/>
      <!-- Grasp kadai -->
      <Action ID="GRASP" obj="kadai"/>
      <!-- Navigate to table (move to right edge of the table) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (place the kadai on the right edge of the table) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>