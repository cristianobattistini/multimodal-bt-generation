<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_napkin -->
      <Action ID="NAVIGATE_TO" obj="blue_napkin"/>
      <!-- Grasp blue_napkin -->
      <Action ID="GRASP" obj="blue_napkin"/>
      <!-- Navigate to table (place in front of cans) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>