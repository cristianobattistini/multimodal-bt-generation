<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to cup -->
      <Action ID="NAVIGATE_TO" obj="cup"/>
      <!-- Grasp cup -->
      <Action ID="GRASP" obj="cup"/>
      <!-- Flip cup upright -->
      <Action ID="FLIP" obj="cup"/>
      <!-- Navigate to light_blue_basin -->
      <Action ID="NAVIGATE_TO" obj="light_blue_basin"/>
      <!-- Place inside light_blue_basin -->
      <Action ID="PLACE_INSIDE" obj="light_blue_basin"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>