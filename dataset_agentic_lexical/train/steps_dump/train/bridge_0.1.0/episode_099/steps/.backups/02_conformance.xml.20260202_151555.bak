<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to shaker -->
      <Action ID="NAVIGATE_TO" obj="shaker"/>
      <!-- Grasp shaker -->
      <Action ID="GRASP" obj="shaker"/>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Place inside pot -->
      <Action ID="PLACE_INSIDE" obj="pot"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>