<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to metal_pot -->
      <Action ID="NAVIGATE_TO" obj="metal_pot"/>
      <!-- Grasp metal_pot -->
      <Action ID="GRASP" obj="metal_pot"/>
      <!-- Navigate to table (place in front of blue_cloth on the table) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>