<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to banana -->
      <Action ID="NAVIGATE_TO" obj="banana"/>
      <!-- Grasp banana -->
      <Action ID="GRASP" obj="banana"/>
      <!-- Navigate to green_cloth -->
      <Action ID="NAVIGATE_TO" obj="green_cloth"/>
      <!-- Place on green_cloth -->
      <Action ID="PLACE_ON_TOP" obj="green_cloth"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>