<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to steel_pan -->
      <Action ID="NAVIGATE_TO" obj="steel_pan"/>
      <!-- Grasp yellow_capsicum -->
      <Action ID="GRASP" obj="yellow_capsicum"/>
      <!-- Navigate to table (place on table corner) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>