<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to strawberry -->
      <Action ID="NAVIGATE_TO" obj="strawberry"/>
      <!-- Grasp strawberry -->
      <Action ID="GRASP" obj="strawberry"/>
      <!-- Navigate to orange_towel -->
      <Action ID="NAVIGATE_TO" obj="orange_towel"/>
      <!-- Place on orange_towel -->
      <Action ID="PLACE_ON_TOP" obj="orange_towel"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>