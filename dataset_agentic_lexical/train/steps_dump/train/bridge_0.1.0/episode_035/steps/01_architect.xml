<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to strawberry -->
      <Action ID="NAVIGATE_TO" obj="strawberry"/>
      <!-- Grasp strawberry -->
      <Action ID="GRASP" obj="strawberry"/>
      <!-- Navigate to green_plate -->
      <Action ID="NAVIGATE_TO" obj="green_plate"/>
      <!-- Place on green_plate -->
      <Action ID="PLACE_ON_TOP" obj="green_plate"/>
      <!-- Navigate to stuffedduck -->
      <Action ID="NAVIGATE_TO" obj="stuffedduck"/>
      <!-- Grasp stuffedduck -->
      <Action ID="GRASP" obj="stuffedduck"/>
      <!-- Navigate to pot_in_sink -->
      <Action ID="NAVIGATE_TO" obj="pot_in_sink"/>
      <!-- Place inside pot_in_sink -->
      <Action ID="PLACE_INSIDE" obj="pot_in_sink"/>
    </Sequence>
  </BehaviorTree>
</root>