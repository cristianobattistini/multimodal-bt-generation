<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pan -->
      <Action ID="NAVIGATE_TO" obj="pan"/>
      <!-- Grasp pan -->
      <Action ID="GRASP" obj="pan"/>
      <!-- Navigate to table (place to the right of bottle and banana) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (to the right of bottle and banana) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>