<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Grasp pot -->
      <Action ID="GRASP" obj="pot"/>
      <!-- Navigate to orange_cloth (place behind cloth on table) -->
      <Action ID="NAVIGATE_TO" obj="orange_cloth"/>
      <!-- Place on orange_cloth -->
      <Action ID="PLACE_ON_TOP" obj="orange_cloth"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>