<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Grab pot -->
      <Action ID="GRAB" obj="pot" />
      <!-- Navigate to stove -->
      <Action ID="NAVIGATE_TO" obj="stove" />
      <!-- Place on stove -->
      <Action ID="PLACE_ON_TOP" obj="stove" />
      <!-- Approach corncob -->
      <Action ID="APPROACH" obj="corncob" />
      <!-- Grasp corncob -->
      <Action ID="GRASP" obj="corncob" />
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Place inside pot -->
      <Action ID="PLACE_INSIDE" obj="pot" />
    </Sequence>
  </BehaviorTree>
</root>
