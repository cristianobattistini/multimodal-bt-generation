<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to corn -->
      <Action ID="NAVIGATE_TO" obj="corn"/>
      <!-- Grasp corn -->
      <Action ID="GRASP" obj="corn"/>
      <!-- Navigate to table (place to the left of the pan) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (left of pan) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>