<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to silver_pot -->
      <Action ID="NAVIGATE_TO" obj="silver_pot"/>
      <!-- Grasp silver_pot -->
      <Action ID="GRASP" obj="silver_pot"/>
      <!-- Navigate to yellow_napkin -->
      <Action ID="NAVIGATE_TO" obj="yellow_napkin"/>
      <!-- Place on yellow_napkin -->
      <Action ID="PLACE_ON_TOP" obj="yellow_napkin"/>
    </Sequence>
  </BehaviorTree>
</root>