<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to bell_pepper -->
      <Action ID="NAVIGATE_TO" obj="bell_pepper"/>
      <!-- Grasp bell_pepper -->
      <Action ID="GRASP" obj="bell_pepper"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (to the right of the yellow towel, bottom area) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>