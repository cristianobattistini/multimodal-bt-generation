<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pink_towel -->
      <Action ID="NAVIGATE_TO" obj="pink_towel"/>
      <!-- Grasp pink_towel -->
      <Action ID="GRASP" obj="pink_towel"/>
      <!-- Navigate to green_mat -->
      <Action ID="NAVIGATE_TO" obj="green_mat"/>
      <!-- Unfold pink_towel -->
      <Action ID="UNFOLD" obj="pink_towel"/>
      <!-- Place on green_mat -->
      <Action ID="PLACE_ON_TOP" obj="green_mat"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>