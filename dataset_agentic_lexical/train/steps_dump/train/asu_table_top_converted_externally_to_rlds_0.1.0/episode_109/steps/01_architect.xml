<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Go to red_cube -->
      <Action ID="GO_TO" obj="red_cube" />
      <!-- Grip red_cube -->
      <Action ID="GRIP" obj="red_cube" />
      <!-- Move to table -->
      <Action ID="MOVE_TO" obj="table" />
      <!-- Lay on table -->
      <Action ID="LAY_ON" obj="table" />
    </Sequence>
  </BehaviorTree>
</root>
