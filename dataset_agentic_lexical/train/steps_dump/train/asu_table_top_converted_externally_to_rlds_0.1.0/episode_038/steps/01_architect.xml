<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to white_box -->
      <Action ID="NAVIGATE_TO" obj="white_box" />
      <!-- Grip white_box -->
      <Action ID="GRIP" obj="white_box" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place white_box on table -->
      <Action ID="PLACE_ON_TOP" obj="white_box" surface="table" />
    </Sequence>
  </BehaviorTree>
</root>
