<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to maroon_square -->
      <Action ID="NAVIGATE_TO" obj="maroon_square"/>
      <!-- Grasp maroon_square -->
      <Action ID="GRASP" obj="maroon_square"/>
      <!-- Navigate to table (place near robot at table edge) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>