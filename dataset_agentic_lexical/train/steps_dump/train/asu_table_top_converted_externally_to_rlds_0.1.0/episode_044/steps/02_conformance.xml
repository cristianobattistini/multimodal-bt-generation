<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to milk_box -->
      <Action ID="NAVIGATE_TO" obj="milk_box" />
      <!-- Grasp milk_box -->
      <Action ID="GRASP" obj="milk_box" />
      <!-- Go to table -->
      <Action ID="GO_TO" obj="table" />
      <!-- Place milk_box on table -->
      <Action ID="PLACE_ON_TOP" obj="milk_box" surface="table" />
    </Sequence>
  </BehaviorTree>
</root>
