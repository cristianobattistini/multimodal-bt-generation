<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pepsi_can -->
      <Action ID="NAVIGATE_TO" obj="pepsi_can"/>
      <!-- Grasp pepsi_can -->
      <Action ID="GRASP" obj="pepsi_can"/>
      <!-- Navigate to blue_plastic_bottle (place next to it on table) -->
      <Action ID="NAVIGATE_TO" obj="blue_plastic_bottle"/>
      <!-- Place on table (set the pepsi_can down beside the blue_plastic_bottle) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>