<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to redbull_can -->
      <Action ID="NAVIGATE_TO" obj="redbull_can"/>
      <!-- Grasp redbull_can -->
      <Action ID="GRASP" obj="redbull_can"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table next to 7up_can -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>