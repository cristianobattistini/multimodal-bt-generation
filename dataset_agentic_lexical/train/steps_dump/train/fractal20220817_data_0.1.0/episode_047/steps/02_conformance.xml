<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to water_bottle -->
      <Action ID="NAVIGATE_TO" obj="water_bottle"/>
      <!-- Grasp water_bottle -->
      <Action ID="GRASP" obj="water_bottle"/>
      <!-- Navigate to paper (place near paper_bowl) -->
      <Action ID="NAVIGATE_TO" obj="paper"/>
      <!-- Place next to paper -->
      <Action ID="PLACE_NEXT_TO" obj="paper"/>
    </Sequence>
  </BehaviorTree>
</root>