<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to water_bottle -->
      <Action ID="NAVIGATE_TO" obj="water_bottle"/>
      <!-- Grasp water_bottle -->
      <Action ID="GRASP" obj="water_bottle"/>
      <!-- Navigate to table (place near paper_bowl) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (near paper_bowl) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>