<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to yellow_plate -->
      <Action ID="NAVIGATE_TO" obj="yellow_plate" />
      <!-- Grasp yellow_plate -->
      <Action ID="GRASP" obj="yellow_plate" />
      <!-- Move to dish_rack -->
      <Action ID="MOVE_TO" obj="dish_rack" />
      <!-- Put in dish_rack -->
      <Action ID="PUT_IN" obj="dish_rack" />
      <!-- Go to white_cup -->
      <Action ID="GO_TO" obj="white_cup" />
      <!-- Grasp white_cup -->
      <Action ID="GRASP" obj="white_cup" />
      <!-- Go to dish_rack -->
      <Action ID="GO_TO" obj="dish_rack" />
      <!-- Insert into dish_rack -->
      <Action ID="INSERT_INTO" obj="dish_rack" />
    </Sequence>
  </BehaviorTree>
</root>
