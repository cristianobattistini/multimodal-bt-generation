<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Go to yellow_plate -->
      <Action ID="GO_TO" obj="yellow_plate" />
      <!-- Grasp yellow_plate -->
      <Action ID="GRASP" obj="yellow_plate" />
      <!-- Move to dish_rack -->
      <Action ID="MOVE_TO" obj="dish_rack" />
      <!-- Set in dish_rack -->
      <Action ID="SET_IN" obj="dish_rack" />
      <!-- Navigate to white_cup -->
      <Action ID="NAVIGATE_TO" obj="white_cup" />
      <!-- Pick white_cup -->
      <Action ID="PICK" obj="white_cup" />
      <!-- Approach dish_rack -->
      <Action ID="APPROACH" obj="dish_rack" />
      <!-- Place white_cup inside dish_rack -->
      <Action ID="PLACE_INSIDE" obj="white_cup" container="dish_rack" />
    </Sequence>
  </BehaviorTree>
</root>
