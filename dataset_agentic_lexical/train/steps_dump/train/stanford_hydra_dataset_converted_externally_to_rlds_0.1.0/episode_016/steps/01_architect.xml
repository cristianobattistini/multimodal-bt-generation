<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to yellow_plate -->
      <Action ID="NAVIGATE_TO" obj="yellow_plate" />
      <!-- Grip yellow_plate -->
      <Action ID="GRIP" obj="yellow_plate" />
      <!-- Approach dish_rack -->
      <Action ID="APPROACH" obj="dish_rack" />
      <!-- Set in dish_rack -->
      <Action ID="SET_IN" obj="dish_rack" />
      <!-- Approach white_mug -->
      <Action ID="APPROACH" obj="white_mug" />
      <!-- Grasp white_mug -->
      <Action ID="GRASP" obj="white_mug" />
      <!-- Go to dish_rack -->
      <Action ID="GO_TO" obj="dish_rack" />
      <!-- Place inside dish_rack -->
      <Action ID="PLACE_INSIDE" obj="dish_rack" />
    </Sequence>
  </BehaviorTree>
</root>
