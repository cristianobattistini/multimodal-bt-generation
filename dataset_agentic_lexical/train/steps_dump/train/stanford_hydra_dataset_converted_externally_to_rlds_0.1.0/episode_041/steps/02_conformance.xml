<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to yellow_plate -->
      <Action ID="MOVE_TO" obj="yellow_plate" />
      <!-- Grip yellow_plate -->
      <Action ID="GRIP" obj="yellow_plate" />
      <!-- Navigate to dish_rack -->
      <Action ID="NAVIGATE_TO" obj="dish_rack" />
      <!-- Place inside dish_rack (upright into slot) -->
      <Action ID="PLACE_INSIDE" obj="dish_rack" />
    </Sequence>
  </BehaviorTree>
</root>
