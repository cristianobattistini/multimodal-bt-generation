<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to white plate -->
      <Action ID="NAVIGATE_TO" obj="white_plate" />
      <!-- Move to bread -->
      <Action ID="MOVE_TO" obj="bread" />
      <!-- Pick bread -->
      <Action ID="PICK" obj="bread" />
    </Sequence>
  </BehaviorTree>
</root>
