<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach bread -->
      <Action ID="APPROACH" obj="bread" />
      <!-- Pick bread -->
      <Action ID="PICK" obj="bread" />
      <!-- Move to plate -->
      <Action ID="MOVE_TO" obj="plate" />
      <!-- Put on plate -->
      <Action ID="PUT_ON" obj="plate" />
    </Sequence>
  </BehaviorTree>
</root>
