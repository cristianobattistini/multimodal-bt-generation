<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to dishwasher -->
      <Action ID="MOVE_TO" obj="dishwasher" />
      <!-- Move to dishwasher_handle -->
      <Action ID="MOVE_TO" obj="dishwasher_handle" />
      <!-- Pick dishwasher_handle -->
      <Action ID="PICK" obj="dishwasher_handle" />
      <!-- Open up dishwasher -->
      <Action ID="OPEN_UP" obj="dishwasher" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
