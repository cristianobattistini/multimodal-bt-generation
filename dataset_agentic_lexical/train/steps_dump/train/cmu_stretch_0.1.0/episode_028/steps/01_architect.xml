<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to top_drawer -->
      <Action ID="NAVIGATE_TO" obj="top_drawer" />
      <!-- Grab top_drawer -->
      <Action ID="GRAB" obj="top_drawer" />
      <!-- Open top_drawer -->
      <Action ID="OPEN" obj="top_drawer" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
