<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Navigate to lid -->
      <Action ID="NAVIGATE_TO" obj="lid" />
      <!-- Grasp lid -->
      <Action ID="GRASP" obj="lid" />
      <!-- Open up lid -->
      <Action ID="OPEN_UP" obj="lid" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
