<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to bread -->
      <Action ID="NAVIGATE_TO" obj="bread"/>
      <!-- Grasp bread -->
      <Action ID="GRASP" obj="bread"/>
      <!-- Navigate to plate -->
      <Action ID="NAVIGATE_TO" obj="plate"/>
      <!-- Place on plate -->
      <Action ID="PLACE_ON_TOP" obj="plate"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
      <!-- Navigate to butter -->
      <Action ID="NAVIGATE_TO" obj="butter"/>
      <!-- Grasp butter -->
      <Action ID="GRASP" obj="butter"/>
      <!-- Navigate to plate -->
      <Action ID="NAVIGATE_TO" obj="plate"/>
      <!-- Place on plate -->
      <Action ID="PLACE_ON_TOP" obj="plate"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
      <!-- Navigate to milk -->
      <Action ID="NAVIGATE_TO" obj="milk"/>
      <!-- Grasp milk -->
      <Action ID="GRASP" obj="milk"/>
      <!-- Navigate to plate -->
      <Action ID="NAVIGATE_TO" obj="plate"/>
      <!-- Place on plate -->
      <Action ID="PLACE_ON_TOP" obj="plate"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>