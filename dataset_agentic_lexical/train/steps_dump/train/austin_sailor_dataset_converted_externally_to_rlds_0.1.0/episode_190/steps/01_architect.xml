<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to fish -->
      <Action ID="MOVE_TO" obj="fish" />
      <!-- Grasp fish -->
      <Action ID="GRASP" obj="fish" />
      <!-- Navigate to frying_pan -->
      <Action ID="NAVIGATE_TO" obj="frying_pan" />
      <!-- Place fish inside frying_pan -->
      <Action ID="PLACE_INSIDE" obj="fish" container="frying_pan" />
      <!-- Go to sausage -->
      <Action ID="GO_TO" obj="sausage" />
      <!-- Pick sausage -->
      <Action ID="PICK" obj="sausage" />
      <!-- Go to frying_pan -->
      <Action ID="GO_TO" obj="frying_pan" />
      <!-- Set in frying_pan -->
      <Action ID="SET_IN" obj="frying_pan" />
      <!-- Move to tomato -->
      <Action ID="MOVE_TO" obj="tomato" />
      <!-- Grasp tomato -->
      <Action ID="GRASP" obj="tomato" />
      <!-- Approach frying_pan -->
      <Action ID="APPROACH" obj="frying_pan" />
      <!-- Place tomato inside frying_pan -->
      <Action ID="PLACE_INSIDE" obj="tomato" container="frying_pan" />
    </Sequence>
  </BehaviorTree>
</root>
