<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Advance to kcup_holder -->
      <Action ID="ADVANCE_TO" obj="kcup_holder" />
      <!-- Open up kcup_holder -->
      <Action ID="OPEN_UP" obj="kcup_holder" />
      <!-- Navigate to kcup -->
      <Action ID="NAVIGATE_TO" obj="kcup" />
      <!-- Pick kcup -->
      <Action ID="PICK" obj="kcup" />
      <!-- Navigate to kcup_holder -->
      <Action ID="NAVIGATE_TO" obj="kcup_holder" />
      <!-- Place kcup inside kcup_holder -->
      <Action ID="PLACE_INSIDE" obj="kcup" container="kcup_holder" />
      <!-- Shut kcup_holder -->
      <Action ID="SHUT" obj="kcup_holder" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
