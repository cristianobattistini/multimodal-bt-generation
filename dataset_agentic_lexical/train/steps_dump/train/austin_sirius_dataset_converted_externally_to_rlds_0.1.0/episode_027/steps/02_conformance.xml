      <Action ID="GRIP" obj="kcup" />
      <!-- Go to kcup_holder -->
      <Action ID="GO_TO" obj="kcup_holder" />
      <!-- Place inside kcup_holder -->
      <Action ID="PLACE_INSIDE" obj="kcup_holder" />
      <!-- Move to kcup_holder -->
      <Action ID="MOVE_TO" obj="kcup_holder" />
      <!-- Shut kcup_holder -->
      <Action ID="SHUT" obj="kcup_holder" />
    </Sequence>
  </BehaviorTree>
</root>
