<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Go to blue_gear -->
      <Action ID="GO_TO" obj="blue_gear" />
      <!-- Grasp blue_gear -->
      <Action ID="GRASP" obj="blue_gear" />
      <!-- Approach right_peg -->
      <Action ID="APPROACH" obj="right_peg" />
      <!-- Place on right_peg -->
      <Action ID="PLACE_ON_TOP" obj="right_peg" />
      <!-- Navigate to red_gear -->
      <Action ID="NAVIGATE_TO" obj="red_gear" />
      <!-- Clamp red_gear -->
      <Action ID="CLAMP" obj="red_gear" />
      <!-- Approach right_peg -->
      <Action ID="APPROACH" obj="right_peg" />
      <!-- Set on right_peg -->
      <Action ID="SET_ON" obj="right_peg" />
    </Sequence>
  </BehaviorTree>
</root>
