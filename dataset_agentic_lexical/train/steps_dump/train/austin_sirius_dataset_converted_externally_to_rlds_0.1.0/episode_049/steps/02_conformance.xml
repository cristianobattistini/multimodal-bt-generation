<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to kcup_holder -->
      <Action ID="NAVIGATE_TO" obj="kcup_holder" />
      <!-- Open kcup_holder -->
      <Action ID="OPEN" obj="kcup_holder" />
      <!-- Approach kcup -->
      <Action ID="APPROACH" obj="kcup" />
      <!-- Grasp kcup -->
      <Action ID="GRASP" obj="kcup" />
      <!-- Navigate to kcup_holder -->
      <Action ID="NAVIGATE_TO" obj="kcup_holder" />
      <!-- Place kcup inside kcup_holder -->
      <Action ID="PLACE_INSIDE" obj="kcup" container="kcup_holder" />
      <!-- Navigate to kcup_holder -->
      <Action ID="NAVIGATE_TO" obj="kcup_holder" />
      <!-- Close kcup_holder -->
      <Action ID="CLOSE" obj="kcup_holder" />
    </Sequence>
  </BehaviorTree>
</root>
