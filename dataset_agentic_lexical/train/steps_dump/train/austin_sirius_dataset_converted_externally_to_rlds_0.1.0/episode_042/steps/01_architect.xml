<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to kcup_holder -->
      <Action ID="MOVE_TO" obj="kcup_holder" />
      <!-- Unseal kcup_holder -->
      <Action ID="UNSEAL" obj="kcup_holder" />
      <!-- Approach kcup -->
      <Action ID="APPROACH" obj="kcup" />
      <!-- Grip kcup -->
      <Action ID="GRIP" obj="kcup" />
      <!-- Move to kcup_holder -->
      <Action ID="MOVE_TO" obj="kcup_holder" />
      <!-- Place kcup inside kcup_holder -->
      <Action ID="PLACE_INSIDE" obj="kcup" container="kcup_holder" />
      <!-- Shut kcup_holder -->
      <Action ID="SHUT" obj="kcup_holder" />
    </Sequence>
  </BehaviorTree>
</root>
