<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to blue_gear -->
      <Action ID="MOVE_TO" obj="blue_gear" />
      <!-- Clamp blue_gear -->
      <Action ID="CLAMP" obj="blue_gear" />
      <!-- Move to right_peg -->
      <Action ID="MOVE_TO" obj="right_peg" />
      <!-- Place on right_peg -->
      <Action ID="PLACE_ON_TOP" obj="right_peg" />
      <!-- Navigate to red_gear -->
      <Action ID="NAVIGATE_TO" obj="red_gear" />
      <!-- Grip red_gear -->
      <Action ID="GRIP" obj="red_gear" />
      <!-- Navigate to right_peg -->
      <Action ID="NAVIGATE_TO" obj="right_peg" />
      <!-- Place on right_peg (stack on blue_gear) -->
      <Action ID="PLACE_ON_TOP" obj="right_peg" />
    </Sequence>
  </BehaviorTree>
</root>
