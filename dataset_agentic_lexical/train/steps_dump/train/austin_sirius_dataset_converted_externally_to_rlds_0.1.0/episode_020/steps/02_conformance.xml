<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Go to blue_gear -->
      <Action ID="GO_TO" obj="blue_gear" />
      <!-- Grasp blue_gear -->
      <Action ID="GRASP" obj="blue_gear" />
      <!-- Navigate to right_peg -->
      <Action ID="NAVIGATE_TO" obj="right_peg" />
      <!-- Set on right_peg -->
      <Action ID="SET_ON" obj="right_peg" />
      <!-- Move to red_gear -->
      <Action ID="MOVE_TO" obj="red_gear" />
      <!-- Grasp red_gear -->
      <Action ID="GRASP" obj="red_gear" />
      <!-- Locomote to right_peg -->
      <Action ID="LOCOMOTE_TO" obj="right_peg" />
      <!-- Place on right_peg (on top of blue_gear) -->
      <Action ID="PLACE_ON_TOP" obj="right_peg" />
    </Sequence>
  </BehaviorTree>
</root>
