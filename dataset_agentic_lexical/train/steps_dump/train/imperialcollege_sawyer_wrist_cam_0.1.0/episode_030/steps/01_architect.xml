<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to small_bowl -->
      <Action ID="NAVIGATE_TO" obj="small_bowl" />
      <!-- Grab small_bowl -->
      <Action ID="GRAB" obj="small_bowl" />
      <!-- Navigate to large_bowl -->
      <Action ID="NAVIGATE_TO" obj="large_bowl" />
      <!-- Place inside large_bowl -->
      <Action ID="PLACE_INSIDE" obj="large_bowl" />
    </Sequence>
  </BehaviorTree>
</root>
