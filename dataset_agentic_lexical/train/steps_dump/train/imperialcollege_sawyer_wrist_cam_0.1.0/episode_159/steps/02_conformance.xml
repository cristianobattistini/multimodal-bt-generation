<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach apple -->
      <Action ID="APPROACH" obj="apple" />
      <!-- Grab apple -->
      <Action ID="GRAB" obj="apple" />
      <!-- Go to pot -->
      <Action ID="GO_TO" obj="pot" />
      <!-- Place apple inside pot -->
      <Action ID="PLACE_INSIDE" obj="apple" container="pot" />
    </Sequence>
  </BehaviorTree>
</root>
