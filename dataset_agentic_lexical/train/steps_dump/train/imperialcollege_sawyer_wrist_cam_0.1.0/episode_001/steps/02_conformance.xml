<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to lid -->
      <Action ID="NAVIGATE_TO" obj="lid" />
      <!-- Grasp lid -->
      <Action ID="GRASP" obj="lid" />
      <!-- Open lid -->
      <Action ID="OPEN" obj="lid" />
      <!-- Ungrasp -->
      <Action ID="UNGRASP" />
    </Sequence>
  </BehaviorTree>
</root>
