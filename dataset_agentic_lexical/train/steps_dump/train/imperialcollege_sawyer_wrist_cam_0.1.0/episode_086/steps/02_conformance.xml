<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Go to cup -->
      <Action ID="GO_TO" obj="cup" />
      <!-- Grip cup -->
      <Action ID="GRIP" obj="cup" />
      <!-- Go to black_hook -->
      <Action ID="GO_TO" obj="black_hook" />
      <!-- Hang cup on black_hook -->
      <Action ID="HANG" obj="black_hook" />
      <!-- Let go -->
      <Action ID="LET_GO" />
    </Sequence>
  </BehaviorTree>
</root>
