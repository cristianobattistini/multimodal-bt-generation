<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to ranch_bottle -->
      <Action ID="NAVIGATE_TO" obj="ranch_bottle" />
      <!-- Grasp ranch_bottle -->
      <Action ID="GRASP" obj="ranch_bottle" />
      <!-- Approach pot -->
      <Action ID="APPROACH" obj="pot" />
      <!-- Insert into pot -->
      <Action ID="INSERT_INTO" obj="pot" />
    </Sequence>
  </BehaviorTree>
</root>
