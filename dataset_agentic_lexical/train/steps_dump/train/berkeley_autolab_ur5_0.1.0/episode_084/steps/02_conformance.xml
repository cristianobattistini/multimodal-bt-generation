<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to ranch_bottle -->
      <Action ID="NAVIGATE_TO" obj="ranch_bottle" />
      <!-- Grip ranch_bottle -->
      <Action ID="GRIP" obj="ranch_bottle" />
      <!-- Go to pot -->
      <Action ID="GO_TO" obj="pot" />
      <!-- Place ranch_bottle inside pot -->
      <Action ID="PLACE_INSIDE" obj="ranch_bottle" container="pot" />
    </Sequence>
  </BehaviorTree>
</root>
