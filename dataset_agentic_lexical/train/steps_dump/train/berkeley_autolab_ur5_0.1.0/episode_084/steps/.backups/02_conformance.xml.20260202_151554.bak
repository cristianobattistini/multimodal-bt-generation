<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to ranch_bottle -->
      <Action ID="NAVIGATE_TO" obj="ranch_bottle"/>
      <!-- Grasp ranch_bottle -->
      <Action ID="GRASP" obj="ranch_bottle"/>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Place inside pot -->
      <Action ID="PLACE_INSIDE" obj="pot"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>