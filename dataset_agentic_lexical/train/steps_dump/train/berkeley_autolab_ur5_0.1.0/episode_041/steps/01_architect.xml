<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach ranch_bottle -->
      <Action ID="APPROACH" obj="ranch_bottle" />
      <!-- Grasp ranch_bottle -->
      <Action ID="GRASP" obj="ranch_bottle" />
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Place ranch_bottle inside pot -->
      <Action ID="PLACE_INSIDE" obj="ranch_bottle" container="pot" />
    </Sequence>
  </BehaviorTree>
</root>
