<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to faucet_handle -->
      <Action ID="NAVIGATE_TO" obj="faucet_handle" />
      <!-- Grasp faucet_handle -->
      <Action ID="GRASP" obj="faucet_handle" />
      <!-- Activate faucet_handle -->
      <Action ID="ACTIVATE" obj="faucet_handle" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
