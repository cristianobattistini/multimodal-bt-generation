<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Go to faucet_handle -->
      <Action ID="GO_TO" obj="faucet_handle" />
      <!-- Grasp faucet_handle -->
      <Action ID="GRASP" obj="faucet_handle" />
      <!-- Activate faucet_handle -->
      <Action ID="ACTIVATE" obj="faucet_handle" />
      <!-- Drop -->
      <Action ID="DROP" />
    </Sequence>
  </BehaviorTree>
</root>
