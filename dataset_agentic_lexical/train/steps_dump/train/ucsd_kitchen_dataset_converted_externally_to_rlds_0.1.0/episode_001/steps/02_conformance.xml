<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach bowl -->
      <Action ID="APPROACH" obj="bowl" />
      <!-- Grasp bowl -->
      <Action ID="GRASP" obj="bowl" />
      <!-- Navigate to kitchen_cabinet -->
      <Action ID="NAVIGATE_TO" obj="kitchen_cabinet" />
      <!-- Place bowl inside kitchen_cabinet -->
      <Action ID="PLACE_INSIDE" obj="bowl" container="kitchen_cabinet" />
    </Sequence>
  </BehaviorTree>
</root>
