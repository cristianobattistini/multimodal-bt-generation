<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach faucet -->
      <Action ID="APPROACH" obj="faucet" />
      <!-- Grasp faucet -->
      <Action ID="GRASP" obj="faucet" />
      <!-- Turn on faucet -->
      <Action ID="TOGGLE_ON" obj="faucet" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
