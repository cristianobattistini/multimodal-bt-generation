<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to drawer -->
      <Action ID="NAVIGATE_TO" obj="drawer"/>
      <!-- Grasp drawer -->
      <Action ID="GRASP" obj="drawer"/>
      <!-- Open drawer -->
      <Action ID="OPEN" obj="drawer"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>