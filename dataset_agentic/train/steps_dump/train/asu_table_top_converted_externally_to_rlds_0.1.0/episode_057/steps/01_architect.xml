<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to maroon_object -->
      <Action ID="NAVIGATE_TO" obj="maroon_object"/>
      <!-- Grasp maroon_object -->
      <Action ID="GRASP" obj="maroon_object"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (place maroon_object on table surface) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>