<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_bottle -->
      <Action ID="NAVIGATE_TO" obj="blue_bottle"/>
      <!-- Grasp blue_bottle -->
      <Action ID="GRASP" obj="blue_bottle"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (put the bottle down on the table) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>