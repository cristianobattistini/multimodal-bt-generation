<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to cutting_board -->
      <Action ID="NAVIGATE_TO" obj="cutting_board"/>
      <!-- Grasp potato -->
      <Action ID="GRASP" obj="potato"/>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Place inside pot -->
      <Action ID="PLACE_INSIDE" obj="pot"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>