<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to red_towel -->
      <Action ID="NAVIGATE_TO" obj="red_towel"/>
      <!-- Grasp red_towel -->
      <Action ID="GRASP" obj="red_towel"/>
      <!-- Navigate to table (place just left of yellow_knife on table) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (just left of yellow_knife) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>