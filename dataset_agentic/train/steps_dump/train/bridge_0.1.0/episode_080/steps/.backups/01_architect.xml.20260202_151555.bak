<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to carrot -->
      <Action ID="NAVIGATE_TO" obj="carrot"/>
      <!-- Grasp carrot -->
      <Action ID="GRASP" obj="carrot"/>
      <!-- Navigate to plate -->
      <Action ID="NAVIGATE_TO" obj="plate"/>
      <!-- Place on plate -->
      <Action ID="PLACE_ON_TOP" obj="plate"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>