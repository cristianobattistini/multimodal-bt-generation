<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to bowl -->
      <Action ID="NAVIGATE_TO" obj="bowl"/>
      <!-- Grasp bowl -->
      <Action ID="GRASP" obj="bowl"/>
      <!-- Navigate to table (place immediately left of blue_towel) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (position: immediately left of blue_towel) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>