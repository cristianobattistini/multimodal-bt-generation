<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Navigate to spoon -->
      <Action ID="NAVIGATE_TO" obj="spoon"/>
      <!-- Grasp spoon -->
      <Action ID="GRASP" obj="spoon"/>
      <!-- Navigate to table (place to the right of pot on table) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>