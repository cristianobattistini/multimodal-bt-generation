<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pepper -->
      <Action ID="NAVIGATE_TO" obj="pepper"/>
      <!-- Grasp pepper -->
      <Action ID="GRASP" obj="pepper"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table to the right of orange_cloth -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>