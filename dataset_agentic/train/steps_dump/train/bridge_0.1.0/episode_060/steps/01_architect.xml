<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Grasp pot -->
      <Action ID="GRASP" obj="pot"/>
      <!-- Navigate to stove -->
      <Action ID="NAVIGATE_TO" obj="stove"/>
      <!-- Place on stove -->
      <Action ID="PLACE_ON_TOP" obj="stove"/>
      <!-- Navigate to corncob -->
      <Action ID="NAVIGATE_TO" obj="corncob"/>
      <!-- Grasp corncob -->
      <Action ID="GRASP" obj="corncob"/>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Place inside pot -->
      <Action ID="PLACE_INSIDE" obj="pot"/>
    </Sequence>
  </BehaviorTree>
</root>