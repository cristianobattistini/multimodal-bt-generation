<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to red_fork -->
      <Action ID="NAVIGATE_TO" obj="red_fork"/>
      <!-- Grasp red_fork -->
      <Action ID="GRASP" obj="red_fork"/>
      <!-- Navigate to table (place directly below eggplant) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>