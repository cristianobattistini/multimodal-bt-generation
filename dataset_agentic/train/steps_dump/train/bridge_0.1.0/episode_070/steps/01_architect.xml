<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to spatula -->
      <Action ID="NAVIGATE_TO" obj="spatula"/>
      <!-- Grasp spatula -->
      <Action ID="GRASP" obj="spatula"/>
      <!-- Navigate to table (place at bottom-left corner) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>