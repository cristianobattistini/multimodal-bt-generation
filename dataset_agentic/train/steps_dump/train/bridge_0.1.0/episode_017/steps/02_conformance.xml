<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to colander -->
      <Action ID="NAVIGATE_TO" obj="colander"/>
      <!-- Grasp colander -->
      <Action ID="GRASP" obj="colander"/>
      <!-- Navigate to table (place in front of red_spoon near front edge) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>