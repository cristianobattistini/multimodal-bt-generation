<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to red_spoon -->
      <Action ID="NAVIGATE_TO" obj="red_spoon"/>
      <!-- Grasp red_spoon -->
      <Action ID="GRASP" obj="red_spoon"/>
      <!-- Navigate to table (place between pan and green_cloth) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>