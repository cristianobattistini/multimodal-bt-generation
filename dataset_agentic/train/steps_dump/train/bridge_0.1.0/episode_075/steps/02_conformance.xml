<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to mushroom -->
      <Action ID="NAVIGATE_TO" obj="mushroom"/>
      <!-- Grasp mushroom -->
      <Action ID="GRASP" obj="mushroom"/>
      <!-- Navigate to blue_cloth -->
      <Action ID="NAVIGATE_TO" obj="blue_cloth"/>
      <!-- Place on top of blue_cloth -->
      <Action ID="PLACE_ON_TOP" obj="blue_cloth"/>
    </Sequence>
  </BehaviorTree>
</root>