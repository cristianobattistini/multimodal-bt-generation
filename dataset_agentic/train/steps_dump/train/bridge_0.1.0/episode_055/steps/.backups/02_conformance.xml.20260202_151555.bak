<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to sushi_top_left -->
      <Action ID="NAVIGATE_TO" obj="sushi_top_left"/>
      <!-- Grasp sushi_top_left -->
      <Action ID="GRASP" obj="sushi_top_left"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (front-center) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>