<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_cloth -->
      <Action ID="NAVIGATE_TO" obj="blue_cloth"/>
      <!-- Grasp blue_cloth -->
      <Action ID="GRASP" obj="blue_cloth"/>
      <!-- Navigate to table (place to the right in front of the cans) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>