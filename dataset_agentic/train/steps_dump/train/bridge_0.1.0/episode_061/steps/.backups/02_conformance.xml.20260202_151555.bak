<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Grasp pot -->
      <Action ID="GRASP" obj="pot"/>
      <!-- Navigate to table (place to the right of the spoon on the table) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table to the right of the spoon -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>