<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to metal_pot -->
      <Action ID="NAVIGATE_TO" obj="metal_pot"/>
      <!-- Grasp orange_object -->
      <Action ID="GRASP" obj="orange_object"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (to the left of the pot) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>