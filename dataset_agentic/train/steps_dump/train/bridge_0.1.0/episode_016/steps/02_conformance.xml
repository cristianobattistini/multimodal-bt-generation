<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to silver_vessel -->
      <Action ID="NAVIGATE_TO" obj="silver_vessel"/>
      <!-- Grasp silver_vessel -->
      <Action ID="GRASP" obj="silver_vessel"/>
      <!-- Navigate to table (place beneath spoon at back_left) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
    </Sequence>
  </BehaviorTree>
</root>