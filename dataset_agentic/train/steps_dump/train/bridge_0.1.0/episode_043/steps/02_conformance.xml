<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to corn -->
      <Action ID="NAVIGATE_TO" obj="corn"/>
      <!-- Grasp corn -->
      <Action ID="GRASP" obj="corn"/>
      <!-- Navigate to metal (place to the left of the pan) -->
      <Action ID="NAVIGATE_TO" obj="metal"/>
      <!-- Place next to metal -->
      <Action ID="PLACE_NEXT_TO" obj="metal"/>
    </Sequence>
  </BehaviorTree>
</root>