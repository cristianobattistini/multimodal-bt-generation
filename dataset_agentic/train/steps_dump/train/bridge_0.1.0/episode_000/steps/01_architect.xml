<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to can -->
      <Action ID="NAVIGATE_TO" obj="can"/>
      <!-- Grasp can -->
      <Action ID="GRASP" obj="can"/>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot"/>
      <!-- Place next to pot -->
      <Action ID="PLACE_NEXT_TO" obj="pot"/>
    </Sequence>
  </BehaviorTree>
</root>