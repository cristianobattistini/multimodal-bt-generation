<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to mug -->
      <Action ID="NAVIGATE_TO" obj="mug"/>
      <!-- Pour into mug -->
      <Action ID="POUR" obj="mug"/>
      <!-- Flip blue_topped_bottle to return upright -->
      <Action ID="FLIP" obj="blue_topped_bottle"/>
    </Sequence>
  </BehaviorTree>
</root>