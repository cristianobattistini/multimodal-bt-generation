<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to green_rice_chip_bag -->
      <Action ID="NAVIGATE_TO" obj="green_rice_chip_bag"/>
      <!-- Grasp green_rice_chip_bag -->
      <Action ID="GRASP" obj="green_rice_chip_bag"/>
      <!-- Navigate to orange (place next to orange on orange) -->
      <Action ID="NAVIGATE_TO" obj="orange"/>
      <!-- Place next to orange -->
      <Action ID="PLACE_NEXT_TO" obj="orange"/>
    </Sequence>
  </BehaviorTree>
</root>