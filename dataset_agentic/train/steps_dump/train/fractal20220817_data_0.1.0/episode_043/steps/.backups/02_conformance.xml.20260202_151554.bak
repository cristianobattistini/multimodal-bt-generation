<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to green_rice_chip_bag -->
      <Action ID="NAVIGATE_TO" obj="green_rice_chip_bag"/>
      <!-- Grasp green_rice_chip_bag -->
      <Action ID="GRASP" obj="green_rice_chip_bag"/>
      <!-- Navigate to table (place near apple on table) -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>