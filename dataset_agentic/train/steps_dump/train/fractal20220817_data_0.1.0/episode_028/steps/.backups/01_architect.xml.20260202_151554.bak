<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to top_drawer -->
      <Action ID="NAVIGATE_TO" obj="top_drawer"/>
      <!-- Navigate to pepsi_can -->
      <Action ID="NAVIGATE_TO" obj="pepsi_can"/>
      <!-- Grasp pepsi_can -->
      <Action ID="GRASP" obj="pepsi_can"/>
      <!-- Navigate to counter -->
      <Action ID="NAVIGATE_TO" obj="counter"/>
      <!-- Place on counter (place on top) -->
      <Action ID="PLACE_ON_TOP" obj="counter"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>