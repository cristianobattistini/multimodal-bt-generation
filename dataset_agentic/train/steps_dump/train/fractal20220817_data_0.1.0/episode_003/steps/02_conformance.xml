<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to coke_can -->
      <Action ID="NAVIGATE_TO" obj="coke_can"/>
      <!-- Grasp coke_can -->
      <Action ID="GRASP" obj="coke_can"/>
      <!-- Navigate to water (place next to water_bottle) -->
      <Action ID="NAVIGATE_TO" obj="water"/>
      <!-- Place next to water -->
      <Action ID="PLACE_NEXT_TO" obj="water"/>
    </Sequence>
  </BehaviorTree>
</root>