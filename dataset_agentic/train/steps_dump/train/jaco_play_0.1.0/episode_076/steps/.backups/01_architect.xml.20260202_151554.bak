<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to gray_bowl -->
      <Action ID="NAVIGATE_TO" obj="gray_bowl"/>
      <!-- Grasp gray_bowl -->
      <Action ID="GRASP" obj="gray_bowl"/>
      <!-- Navigate to oven -->
      <Action ID="NAVIGATE_TO" obj="oven"/>
      <!-- Place inside oven (on oven tray) -->
      <Action ID="PLACE_INSIDE" obj="oven"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>