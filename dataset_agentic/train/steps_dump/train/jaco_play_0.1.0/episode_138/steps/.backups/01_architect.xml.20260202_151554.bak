<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to square_bread -->
      <Action ID="NAVIGATE_TO" obj="square_bread"/>
      <!-- Grasp square_bread -->
      <Action ID="GRASP" obj="square_bread"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (clear area avoiding fruits) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>