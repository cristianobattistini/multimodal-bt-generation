<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to cup -->
      <Action ID="NAVIGATE_TO" obj="cup"/>
      <!-- Grasp cup -->
      <Action ID="GRASP" obj="cup"/>
      <!-- Navigate to wall_hook -->
      <Action ID="NAVIGATE_TO" obj="wall_hook"/>
      <!-- Hang cup on wall_hook -->
      <Action ID="HANG" obj="wall_hook"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>