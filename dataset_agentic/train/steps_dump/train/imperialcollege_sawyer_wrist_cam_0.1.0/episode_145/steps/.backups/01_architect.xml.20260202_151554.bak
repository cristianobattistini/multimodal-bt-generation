<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to red_bottle -->
      <Action ID="NAVIGATE_TO" obj="red_bottle"/>
      <!-- Grasp red_bottle -->
      <Action ID="GRASP" obj="red_bottle"/>
      <!-- Navigate to white_mug -->
      <Action ID="NAVIGATE_TO" obj="white_mug"/>
      <!-- Pour into white_mug -->
      <Action ID="POUR" obj="white_mug"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>