<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to toast_slice -->
      <Action ID="NAVIGATE_TO" obj="toast_slice"/>
      <!-- Grasp toast_slice -->
      <Action ID="GRASP" obj="toast_slice"/>
      <!-- Navigate to toaster_slot -->
      <Action ID="NAVIGATE_TO" obj="toaster_slot"/>
      <!-- Place inside toaster_slot -->
      <Action ID="PLACE_INSIDE" obj="toaster_slot"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>