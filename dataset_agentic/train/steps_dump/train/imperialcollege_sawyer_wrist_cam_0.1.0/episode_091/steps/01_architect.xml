<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to slice_of_toast -->
      <Action ID="NAVIGATE_TO" obj="slice_of_toast"/>
      <!-- Grasp slice_of_toast -->
      <Action ID="GRASP" obj="slice_of_toast"/>
      <!-- Navigate to toaster_slot -->
      <Action ID="NAVIGATE_TO" obj="toaster_slot"/>
      <!-- Place inside toaster_slot (insert toast into slot until seated) -->
      <Action ID="PLACE_INSIDE" obj="toaster_slot"/>
    </Sequence>
  </BehaviorTree>
</root>