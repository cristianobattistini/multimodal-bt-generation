<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to bowl_top -->
      <Action ID="NAVIGATE_TO" obj="bowl_top"/>
      <!-- Grasp bowl_top -->
      <Action ID="GRASP" obj="bowl_top"/>
      <!-- Navigate to bowl_bottom -->
      <Action ID="NAVIGATE_TO" obj="bowl_bottom"/>
      <!-- Place inside bowl_bottom -->
      <Action ID="PLACE_INSIDE" obj="bowl_bottom"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>