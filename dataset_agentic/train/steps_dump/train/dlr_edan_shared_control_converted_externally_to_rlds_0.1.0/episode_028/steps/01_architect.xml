<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to clear_bowl on small_side_table -->
      <Action ID="NAVIGATE_TO" obj="clear_bowl"/>
      <!-- Navigate to yellow_ball (reach into bowl) -->
      <Action ID="NAVIGATE_TO" obj="yellow_ball"/>
      <!-- Grasp yellow_ball -->
      <Action ID="GRASP" obj="yellow_ball"/>
    </Sequence>
  </BehaviorTree>
</root>