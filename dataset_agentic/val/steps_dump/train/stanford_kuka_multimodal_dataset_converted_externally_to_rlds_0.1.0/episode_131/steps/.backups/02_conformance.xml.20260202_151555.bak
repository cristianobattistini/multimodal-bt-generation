<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to hole -->
      <Action ID="NAVIGATE_TO" obj="hole"/>
      <!-- Place peg inside hole (align and lower straight down) -->
      <Action ID="PLACE_INSIDE" obj="hole"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>