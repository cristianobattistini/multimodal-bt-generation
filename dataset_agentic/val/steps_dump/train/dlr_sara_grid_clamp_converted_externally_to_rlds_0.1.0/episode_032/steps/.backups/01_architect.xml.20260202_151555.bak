<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp"/>
      <!-- Grasp grid_clamp -->
      <Action ID="GRASP" obj="grid_clamp"/>
      <!-- Navigate to wooden_board -->
      <Action ID="NAVIGATE_TO" obj="wooden_board"/>
      <!-- Place on wooden_board (align with grid slot) -->
      <Action ID="PLACE_ON_TOP" obj="wooden_board"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>