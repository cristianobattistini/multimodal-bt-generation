<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp"/>
      <!-- Grasp grid_clamp -->
      <Action ID="GRASP" obj="grid_clamp"/>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Place on table (grid area) -->
      <Action ID="PLACE_ON_TOP" obj="table"/>
      <!-- Release -->
      <Action ID="RELEASE"/>
    </Sequence>
  </BehaviorTree>
</root>