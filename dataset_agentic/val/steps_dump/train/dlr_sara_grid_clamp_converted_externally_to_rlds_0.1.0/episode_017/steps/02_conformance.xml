<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp"/>
      <!-- Grasp grid_clamp -->
      <Action ID="GRASP" obj="grid_clamp"/>
      <!-- Navigate to grid_plate -->
      <Action ID="NAVIGATE_TO" obj="grid_plate"/>
      <!-- Place inside grid_plate -->
      <Action ID="PLACE_INSIDE" obj="grid_plate"/>
    </Sequence>
  </BehaviorTree>
</root>