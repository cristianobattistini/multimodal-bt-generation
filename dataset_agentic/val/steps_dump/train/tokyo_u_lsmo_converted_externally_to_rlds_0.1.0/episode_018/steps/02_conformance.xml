<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to book_stack to move around the obstacle -->
      <Action ID="NAVIGATE_TO" obj="book_stack"/>
      <!-- Navigate to blue_pen approaching from the side of the book_stack -->
      <Action ID="NAVIGATE_TO" obj="blue_pen"/>
      <!-- Grasp blue_pen -->
      <Action ID="GRASP" obj="blue_pen"/>
    </Sequence>
  </BehaviorTree>
</root>