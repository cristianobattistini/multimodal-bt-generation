<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate around books to dark_container -->
      <Action ID="NAVIGATE_TO" obj="dark_container"/>
      <!-- Navigate to scissors inside the container -->
      <Action ID="NAVIGATE_TO" obj="scissors"/>
      <!-- Grasp scissors -->
      <Action ID="GRASP" obj="scissors"/>
    </Sequence>
  </BehaviorTree>
</root>