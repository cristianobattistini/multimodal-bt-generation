<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to table while avoiding book_stack obstacle -->
      <Action ID="NAVIGATE_TO" obj="table"/>
      <!-- Navigate to blue_pen approaching from a path around the book_stack -->
      <Action ID="NAVIGATE_TO" obj="blue_pen"/>
      <!-- Grasp blue_pen -->
      <Action ID="GRASP" obj="blue_pen"/>
    </Sequence>
  </BehaviorTree>
</root>