<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to green_rice_chip_bag -->
      <Action ID="MOVE_TO" obj="green_rice_chip_bag" />
      <!-- Grasp green_rice_chip_bag -->
      <Action ID="GRASP" obj="green_rice_chip_bag" />
      <Sequence>
        <!-- Check rxbar_blueberry is reachable before NAVIGATE_TO -->
        <Condition ID="IS_REACHABLE" obj="rxbar_blueberry" />
        <!-- Timeout: NAVIGATE_TO rxbar_blueberry within 20s -->
        <Timeout msec="20000">
          <!-- Navigate to rxbar_blueberry -->
          <Action ID="GO_TO" obj="rxbar_blueberry" />
        </Timeout>
      </Sequence>
      <!-- Go to rxbar_blueberry -->
      <Action ID="PLACE_NEXT_TO" obj="rxbar_blueberry" />
    </Sequence>
  </BehaviorTree>
</root>
