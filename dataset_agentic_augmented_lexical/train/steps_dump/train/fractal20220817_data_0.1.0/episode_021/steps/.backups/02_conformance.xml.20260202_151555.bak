<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachBottomDrawer -->
      <SubTree ID="ApproachBottomDrawer" />
      <!-- Grasp blue_chip_bag -->
      <Action ID="GRASP" obj="blue_chip_bag" />
      <!-- Navigate to counter -->
      <Action ID="NAVIGATE_TO" obj="counter" />
      <!-- Place on counter -->
      <Action ID="PLACE_ON_TOP" obj="counter" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachBottomDrawer -->
  <BehaviorTree ID="ApproachBottomDrawer">
    <Sequence>
      <!-- Navigate to bottom_drawer -->
      <Action ID="NAVIGATE_TO" obj="bottom_drawer" />
      <!-- Navigate to blue_chip_bag -->
      <Action ID="NAVIGATE_TO" obj="blue_chip_bag" />
    </Sequence>
  </BehaviorTree>
</root>
