<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pepsi_can -->
      <Action ID="NAVIGATE_TO" obj="pepsi_can" />
      <!-- Grasp pepsi_can -->
      <Action ID="GRASP" obj="pepsi_can" />
      <Sequence>
        <!-- Check blue_plastic_bottle is visible before NAVIGATE_TO -->
        <Condition ID="IS_VISIBLE" obj="blue_plastic_bottle" />
        <!-- Navigate to blue_plastic_bottle -->
        <Action ID="NAVIGATE_TO" obj="blue_plastic_bottle" />
      </Sequence>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
