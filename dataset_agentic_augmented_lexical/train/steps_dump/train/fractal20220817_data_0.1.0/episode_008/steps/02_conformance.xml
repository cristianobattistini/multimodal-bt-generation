<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to fridge -->
      <Action ID="NAVIGATE_TO" obj="fridge" />
      <!-- Move to 7up_can -->
      <Action ID="MOVE_TO" obj="7up_can" />
      <!-- Fallback: if GRASP 7up_can fails, try PUSH, FLIP to reposition -->
      <Fallback>
        <!-- Grasp 7up_can -->
        <Action ID="PICK" obj="7up_can" />
        <Sequence>
          <!-- Push 7up_can forward toward front edge of table -->
          <Action ID="NUDGE" obj="7up_can" />
          <!-- Grasp 7up_can -->
          <Action ID="GRIP" obj="7up_can" />
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
