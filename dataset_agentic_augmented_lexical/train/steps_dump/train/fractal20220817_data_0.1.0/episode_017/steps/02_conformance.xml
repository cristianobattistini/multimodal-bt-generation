<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach green_jalapeno_chip_bag -->
      <Action ID="APPROACH" obj="green_jalapeno_chip_bag" />
      <!-- Pick green_jalapeno_chip_bag -->
      <Action ID="PICK" obj="green_jalapeno_chip_bag" />
      <!-- Approach top_drawer -->
      <Action ID="APPROACH" obj="top_drawer" />
      <!-- Retry PLACE_INSIDE top_drawer up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Place inside top_drawer -->
        <Action ID="PLACE_INSIDE" obj="top_drawer" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
