<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachTopDrawer -->
      <SubTree ID="ApproachTopDrawer" />
      <!-- Open top_drawer -->
      <Action ID="OPEN" obj="top_drawer" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachTopDrawer -->
  <BehaviorTree ID="ApproachTopDrawer">
    <Sequence>
      <!-- Navigate to top_drawer -->
      <Action ID="NAVIGATE_TO" obj="top_drawer" />
      <!-- Grasp top_drawer -->
      <Action ID="GRASP" obj="top_drawer" />
    </Sequence>
  </BehaviorTree>
</root>
