<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to coke_can -->
      <Action ID="NAVIGATE_TO" obj="coke_can" />
      <!-- Grasp coke_can -->
      <Action ID="GRASP" obj="coke_can" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Retry PLACE_ON_TOP table up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Place on table -->
        <Action ID="PLACE_ON_TOP" obj="table" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
