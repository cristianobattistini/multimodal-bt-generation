<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to white_bowl -->
      <Action ID="NAVIGATE_TO" obj="white_bowl" />
      <Sequence>
        <!-- Check apple is reachable before GRASP -->
        <Condition ID="IS_REACHABLE" obj="apple" />
        <!-- Grasp apple -->
        <Action ID="GRASP" obj="apple" />
      </Sequence>
    </Sequence>
  </BehaviorTree>
</root>
