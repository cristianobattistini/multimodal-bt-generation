<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachWhitePlate -->
      <SubTree ID="ApproachWhitePlate" />
      <!-- Go to red_plate -->
      <Action ID="GO_TO" obj="red_plate" />
      <!-- Set on red_plate -->
      <Action ID="SET_ON" obj="red_plate" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachWhitePlate -->
  <BehaviorTree ID="ApproachWhitePlate">
    <Sequence>
      <!-- Go to white_plate -->
      <Action ID="GO_TO" obj="white_plate" />
      <!-- Grasp white_plate -->
      <Action ID="GRASP" obj="white_plate" />
    </Sequence>
  </BehaviorTree>
</root>
