<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to apple -->
      <Action ID="NAVIGATE_TO" obj="apple" />
      <!-- Grasp apple -->
      <Action ID="GRASP" obj="apple" />
      <Sequence>
        <!-- Check pot is reachable before NAVIGATE_TO -->
        <Condition ID="IS_REACHABLE" obj="pot" />
        <!-- Navigate to pot -->
        <Action ID="NAVIGATE_TO" obj="pot" />
      </Sequence>
      <!-- Place inside pot -->
      <Action ID="PLACE_INSIDE" obj="pot" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
