<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to toast -->
      <Action ID="NAVIGATE_TO" obj="toast" />
      <!-- Retry GRASP toast up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Grasp toast -->
        <Action ID="GRASP" obj="toast" />
      </RetryUntilSuccessful>
      <!-- Navigate to toaster_slot -->
      <Action ID="NAVIGATE_TO" obj="toaster_slot" />
      <!-- Place inside toaster_slot -->
      <Action ID="PLACE_INSIDE" obj="toaster_slot" />
    </Sequence>
  </BehaviorTree>
</root>
