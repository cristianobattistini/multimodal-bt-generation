<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO lid within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to lid -->
        <Action ID="NAVIGATE_TO" obj="lid" />
      </Timeout>
      <!-- Grasp lid -->
      <Action ID="GRASP" obj="lid" />
      <!-- Open lid -->
      <Action ID="OPEN" obj="lid" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
