<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO milk_box within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to milk_box -->
        <Action ID="NAVIGATE_TO" obj="milk_box" />
      </Timeout>
      <!-- Grasp milk_box -->
      <Action ID="GRASP" obj="milk_box" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
