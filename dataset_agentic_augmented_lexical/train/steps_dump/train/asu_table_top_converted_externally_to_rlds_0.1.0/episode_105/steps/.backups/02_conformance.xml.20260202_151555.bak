<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to bread -->
      <Action ID="NAVIGATE_TO" obj="bread" />
      <!-- Grasp bread -->
      <Action ID="GRASP" obj="bread" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Fallback: if PLACE_ON_TOP table fails, try alternative to reposition -->
      <Fallback>
        <!-- Retry PLACE_ON_TOP table up to 3 attempts -->
        <RetryUntilSuccessful num_attempts="3">
          <!-- Place on table -->
          <Action ID="PLACE_ON_TOP" obj="table" />
        </RetryUntilSuccessful>
        <Sequence>
          <!-- Push table forward toward front edge of table -->
          <Action ID="PUSH" obj="table" />
          <!-- Place on table -->
          <Action ID="PLACE_ON_TOP" obj="table" />
        </Sequence>
      </Fallback>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
