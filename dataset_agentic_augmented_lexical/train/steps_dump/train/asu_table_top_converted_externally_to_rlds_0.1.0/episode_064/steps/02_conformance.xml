<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach red_object -->
      <Action ID="APPROACH" obj="red_object" />
      <!-- Retry GRASP red_object up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Grasp red_object -->
        <Action ID="GRASP" obj="red_object" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
