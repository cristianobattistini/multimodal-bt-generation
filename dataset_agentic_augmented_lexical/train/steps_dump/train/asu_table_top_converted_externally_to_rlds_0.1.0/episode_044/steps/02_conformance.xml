<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to milk_box -->
      <Action ID="NAVIGATE_TO" obj="milk_box" />
      <!-- Grip milk_box -->
      <Action ID="GRIP" obj="milk_box" />
      <!-- Timeout: NAVIGATE_TO table within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to table -->
        <Action ID="NAVIGATE_TO" obj="table" />
      </Timeout>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="milk_box" surface="table" />
    </Sequence>
  </BehaviorTree>
</root>
