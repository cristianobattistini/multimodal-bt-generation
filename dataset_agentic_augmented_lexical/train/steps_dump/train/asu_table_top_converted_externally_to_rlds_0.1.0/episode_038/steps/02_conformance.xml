<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check white_box is reachable before NAVIGATE_TO -->
      <Condition ID="IS_REACHABLE" obj="white_box" />
      <!-- Timeout: NAVIGATE_TO white_box within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to white_box -->
        <Action ID="MOVE_TO" obj="white_box" />
      </Timeout>
      <!-- Move to white_box -->
      <Action ID="GRIP" obj="white_box" />
      <!-- Grip white_box -->
      <Action ID="GO_TO" obj="table" />
      <!-- Go to table -->
      <Action ID="PUT_ON" obj="table" />
    </Sequence>
  </BehaviorTree>
</root>
