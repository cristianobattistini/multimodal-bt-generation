<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to red_object -->
      <Action ID="NAVIGATE_TO" obj="red_object" />
      <Sequence>
        <!-- Check red_object is visible before GRASP -->
        <Condition ID="IS_VISIBLE" obj="red_object" />
        <!-- Grasp red_object -->
        <Action ID="GRASP" obj="red_object" />
      </Sequence>
    </Sequence>
  </BehaviorTree>
</root>
