<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO maroon_cube within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to maroon_cube -->
        <Action ID="NAVIGATE_TO" obj="maroon_cube" />
      </Timeout>
      <!-- Grasp maroon_cube -->
      <Action ID="PICK" obj="maroon_cube" />
      <!-- Pick maroon_cube -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
    </Sequence>
  </BehaviorTree>
</root>
