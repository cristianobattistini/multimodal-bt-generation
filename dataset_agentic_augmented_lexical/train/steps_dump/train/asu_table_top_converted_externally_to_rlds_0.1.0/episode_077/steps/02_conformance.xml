<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to coke_bottle -->
      <Action ID="NAVIGATE_TO" obj="coke_bottle" />
      <!-- Retry PUSH coke_bottle up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Push coke_bottle forward toward front edge of table -->
        <Action ID="PUSH" obj="coke_bottle" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
