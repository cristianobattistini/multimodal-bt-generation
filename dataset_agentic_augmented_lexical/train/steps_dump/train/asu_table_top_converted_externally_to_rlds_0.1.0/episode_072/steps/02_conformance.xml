<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to milk_carton -->
      <Action ID="NAVIGATE_TO" obj="milk_carton" />
      <!-- Retry GRASP milk_carton up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Grasp milk_carton -->
        <Action ID="GRASP" obj="milk_carton" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
