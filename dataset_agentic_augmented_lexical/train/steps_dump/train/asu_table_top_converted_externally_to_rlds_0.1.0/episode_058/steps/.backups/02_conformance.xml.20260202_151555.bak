<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to white_box -->
      <Action ID="NAVIGATE_TO" obj="white_box" />
      <Sequence>
        <!-- Check white_box is visible before GRASP -->
        <Condition ID="IS_VISIBLE" obj="white_box" />
        <!-- Grasp white_box -->
        <Action ID="GRASP" obj="white_box" />
      </Sequence>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
