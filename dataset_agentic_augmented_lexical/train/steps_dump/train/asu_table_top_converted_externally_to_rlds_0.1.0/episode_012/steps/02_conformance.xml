<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to red_cube -->
      <Action ID="NAVIGATE_TO" obj="red_cube" />
      <!-- Fallback: if GRASP red_cube fails, try alternative to reposition -->
      <Fallback>
        <!-- Retry GRASP red_cube up to 3 attempts -->
        <RetryUntilSuccessful num_attempts="3">
          <!-- Grasp red_cube -->
          <Action ID="GRASP" obj="red_cube" />
        </RetryUntilSuccessful>
        <Sequence>
          <!-- Push red_cube forward toward front edge of table -->
          <Action ID="PUSH" obj="red_cube" />
          <!-- Grasp red_cube -->
          <Action ID="GRASP" obj="red_cube" />
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
