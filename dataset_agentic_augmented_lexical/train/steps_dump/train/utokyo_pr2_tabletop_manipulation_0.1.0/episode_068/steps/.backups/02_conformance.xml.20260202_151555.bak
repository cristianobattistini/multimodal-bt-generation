<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grape -->
      <Action ID="NAVIGATE_TO" obj="grape" />
      <!-- Grasp grape -->
      <Action ID="GRASP" obj="grape" />
      <!-- Navigate to plate -->
      <Action ID="NAVIGATE_TO" obj="plate" />
      <!-- Retry PLACE_ON_TOP plate up to 4 attempts -->
      <RetryUntilSuccessful num_attempts="4">
        <!-- Place on plate -->
        <Action ID="PLACE_ON_TOP" obj="plate" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
