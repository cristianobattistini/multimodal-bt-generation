<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <Sequence>
        <!-- Check bread is reachable before NAVIGATE_TO -->
        <Condition ID="IS_REACHABLE" obj="bread" />
        <!-- Navigate to bread -->
        <Action ID="NAVIGATE_TO" obj="bread" />
      </Sequence>
      <!-- Grasp bread -->
      <Action ID="GRASP" obj="bread" />
    </Sequence>
  </BehaviorTree>
</root>
