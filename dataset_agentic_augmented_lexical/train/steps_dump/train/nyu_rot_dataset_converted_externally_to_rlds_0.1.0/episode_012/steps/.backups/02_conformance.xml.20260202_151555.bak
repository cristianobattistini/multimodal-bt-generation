<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to cup -->
      <Action ID="NAVIGATE_TO" obj="cup" />
      <!-- Retry PLACE_INSIDE cup up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Place inside cup -->
        <Action ID="PLACE_INSIDE" obj="cup" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
