<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to door -->
      <Action ID="NAVIGATE_TO" obj="door" />
      <Sequence>
        <!-- Check door is open before CLOSE -->
        <Condition ID="IS_OPEN" obj="door" />
        <!-- Close door -->
        <Action ID="CLOSE" obj="door" />
      </Sequence>
    </Sequence>
  </BehaviorTree>
</root>
