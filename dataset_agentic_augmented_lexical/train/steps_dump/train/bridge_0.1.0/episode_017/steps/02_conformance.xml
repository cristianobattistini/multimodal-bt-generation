<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachColander -->
      <SubTree ID="ApproachColander" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Set on table -->
      <Action ID="SET_ON" obj="table" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachColander -->
  <BehaviorTree ID="ApproachColander">
    <Sequence>
      <!-- Navigate to colander -->
      <Action ID="NAVIGATE_TO" obj="colander" />
      <!-- Grasp colander -->
      <Action ID="GRASP" obj="colander" />
    </Sequence>
  </BehaviorTree>
</root>
