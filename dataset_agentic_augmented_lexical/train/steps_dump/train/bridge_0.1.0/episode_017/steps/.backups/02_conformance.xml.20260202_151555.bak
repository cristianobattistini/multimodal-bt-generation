<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachColander -->
      <SubTree ID="ApproachColander" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachColander -->
  <BehaviorTree ID="ApproachColander">
    <Sequence>
      <!-- Navigate to colander -->
      <Action ID="NAVIGATE_TO" obj="colander" />
      <!-- Grasp colander -->
      <Action ID="GRASP" obj="colander" />
    </Sequence>
  </BehaviorTree>
</root>
