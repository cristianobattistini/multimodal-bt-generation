<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachCan -->
      <SubTree ID="ApproachCan" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachCan -->
  <BehaviorTree ID="ApproachCan">
    <Sequence>
      <!-- Navigate to can -->
      <Action ID="NAVIGATE_TO" obj="can" />
      <!-- Grasp can -->
      <Action ID="GRASP" obj="can" />
    </Sequence>
  </BehaviorTree>
</root>
