<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to knife -->
      <Action ID="NAVIGATE_TO" obj="knife" />
      <!-- Grasp knife -->
      <Action ID="GRASP" obj="knife" />
      <!-- Timeout: NAVIGATE_TO cutting_board within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to cutting_board -->
        <Action ID="NAVIGATE_TO" obj="cutting_board" />
      </Timeout>
      <!-- Place on cutting_board -->
      <Action ID="PLACE_ON_TOP" obj="cutting_board" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
