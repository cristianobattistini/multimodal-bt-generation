<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to metal_pot -->
      <Action ID="NAVIGATE_TO" obj="metal_pot" />
      <!-- Fallback: if GRASP metal_pot fails, try PUSH, FLIP to reposition -->
      <Fallback>
        <!-- Grasp metal_pot -->
        <Action ID="GRASP" obj="metal_pot" />
        <Sequence>
          <!-- Push metal_pot forward toward front edge of table -->
          <Action ID="PUSH" obj="metal_pot" />
          <!-- Grasp metal_pot -->
          <Action ID="GRASP" obj="metal_pot" />
        </Sequence>
      </Fallback>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
