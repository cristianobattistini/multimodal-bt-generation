<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to broccoli -->
      <Action ID="NAVIGATE_TO" obj="broccoli" />
      <!-- Retry GRASP broccoli up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Timeout: GRASP broccoli within 20s -->
        <Timeout msec="20000">
          <!-- Grasp broccoli -->
          <Action ID="GRASP" obj="broccoli" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Place inside pot -->
      <Action ID="PLACE_INSIDE" obj="pot" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
