<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to broccoli -->
      <Action ID="MOVE_TO" obj="broccoli" />
      <!-- Grip broccoli -->
      <Action ID="GRIP" obj="broccoli" />
      <!-- Timeout: NAVIGATE_TO metal_pot within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to metal_pot -->
        <Action ID="NAVIGATE_TO" obj="metal_pot" />
      </Timeout>
      <!-- Place inside metal_pot -->
      <Action ID="PLACE_INSIDE" obj="broccoli" container="metal_pot" />
    </Sequence>
  </BehaviorTree>
</root>
