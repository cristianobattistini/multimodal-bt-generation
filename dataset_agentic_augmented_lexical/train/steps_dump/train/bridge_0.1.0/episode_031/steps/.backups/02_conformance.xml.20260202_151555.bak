<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to peeler -->
      <Action ID="NAVIGATE_TO" obj="peeler" />
      <!-- Retry GRASP peeler up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Grasp peeler -->
        <Action ID="GRASP" obj="peeler" />
      </RetryUntilSuccessful>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
