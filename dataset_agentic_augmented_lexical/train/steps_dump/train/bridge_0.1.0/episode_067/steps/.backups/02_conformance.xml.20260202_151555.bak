<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to mushroom -->
      <Action ID="NAVIGATE_TO" obj="mushroom" />
      <!-- Retry GRASP mushroom up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Timeout: GRASP mushroom within 15s -->
        <Timeout msec="15000">
          <!-- Grasp mushroom -->
          <Action ID="GRASP" obj="mushroom" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Navigate to tiffin_box -->
      <Action ID="NAVIGATE_TO" obj="tiffin_box" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
