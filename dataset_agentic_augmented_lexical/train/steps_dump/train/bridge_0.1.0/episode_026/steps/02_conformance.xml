<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to toolbox_drawer -->
      <Action ID="NAVIGATE_TO" obj="toolbox_drawer" />
      <!-- Navigate to red_srewdriver -->
      <Action ID="NAVIGATE_TO" obj="red_srewdriver" />
      <!-- Fallback: if GRASP red_srewdriver fails, try PUSH, FLIP to reposition -->
      <Fallback>
        <!-- Grasp red_srewdriver -->
        <Action ID="GRASP" obj="red_srewdriver" />
        <Sequence>
          <!-- Push red_srewdriver forward toward front edge of table -->
          <Action ID="PUSH" obj="red_srewdriver" />
          <!-- Grasp red_srewdriver -->
          <Action ID="GRASP" obj="red_srewdriver" />
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
