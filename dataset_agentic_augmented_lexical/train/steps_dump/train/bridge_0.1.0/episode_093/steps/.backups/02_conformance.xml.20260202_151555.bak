<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to yellow_brush -->
      <Action ID="NAVIGATE_TO" obj="yellow_brush" />
      <!-- Grasp yellow_brush -->
      <Action ID="GRASP" obj="yellow_brush" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Check table is holding before PLACE_ON_TOP -->
      <Condition ID="IS_HOLDING" obj="table" />
      <!-- Retry PLACE_ON_TOP table up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Place on table -->
        <Action ID="PLACE_ON_TOP" obj="table" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
