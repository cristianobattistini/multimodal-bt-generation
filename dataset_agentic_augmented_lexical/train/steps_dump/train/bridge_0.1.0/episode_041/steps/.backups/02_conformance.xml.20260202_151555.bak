<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to carrot -->
      <Action ID="NAVIGATE_TO" obj="carrot" />
      <!-- Check carrot is reachable before GRASP -->
      <Condition ID="IS_REACHABLE" obj="carrot" />
      <!-- Fallback: if GRASP carrot fails, try alternative to reposition -->
      <Fallback>
        <!-- Retry GRASP carrot up to 3 attempts -->
        <RetryUntilSuccessful num_attempts="3">
          <!-- Grasp carrot -->
          <Action ID="GRASP" obj="carrot" />
        </RetryUntilSuccessful>
        <Sequence>
          <!-- Push carrot forward toward front edge of table -->
          <Action ID="PUSH" obj="carrot" />
          <!-- Grasp carrot -->
          <Action ID="GRASP" obj="carrot" />
        </Sequence>
      </Fallback>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Place inside pot -->
      <Action ID="PLACE_INSIDE" obj="pot" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
