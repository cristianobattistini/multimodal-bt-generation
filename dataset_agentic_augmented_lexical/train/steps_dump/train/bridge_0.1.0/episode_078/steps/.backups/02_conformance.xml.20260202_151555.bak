<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to utensil -->
      <Action ID="NAVIGATE_TO" obj="utensil" />
      <!-- Check utensil is reachable before GRASP -->
      <Condition ID="IS_REACHABLE" obj="utensil" />
      <!-- Fallback: if GRASP utensil fails, try alternative to reposition -->
      <Fallback>
        <!-- Grasp utensil -->
        <Action ID="GRASP" obj="utensil" />
        <Sequence>
          <!-- Push utensil forward toward front edge of table -->
          <Action ID="PUSH" obj="utensil" />
          <!-- Grasp utensil -->
          <Action ID="GRASP" obj="utensil" />
        </Sequence>
      </Fallback>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
