<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to green_cup -->
      <Action ID="NAVIGATE_TO" obj="green_cup" />
      <!-- Check green_cup is reachable before GRASP -->
      <Condition ID="IS_REACHABLE" obj="green_cup" />
      <!-- Retry GRASP green_cup up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Timeout: GRASP green_cup within 10s -->
        <Timeout msec="10000">
          <!-- Grasp green_cup -->
          <Action ID="GRASP" obj="green_cup" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Navigate to oven -->
      <Action ID="NAVIGATE_TO" obj="oven" />
      <!-- Place inside oven -->
      <Action ID="PLACE_INSIDE" obj="oven" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
