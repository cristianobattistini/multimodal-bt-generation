<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to apple_fruit -->
      <Action ID="NAVIGATE_TO" obj="apple_fruit" />
      <!-- Retry GRASP apple_fruit up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Timeout: GRASP apple_fruit within 10s -->
        <Timeout msec="10000">
          <!-- Grasp apple_fruit -->
          <Action ID="GRASP" obj="apple_fruit" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Navigate to sink -->
      <Action ID="NAVIGATE_TO" obj="sink" />
      <!-- Place inside sink -->
      <Action ID="PLACE_INSIDE" obj="sink" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
