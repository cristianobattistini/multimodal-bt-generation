<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO square_bread within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to square_bread -->
        <Action ID="NAVIGATE_TO" obj="square_bread" />
      </Timeout>
      <!-- Grasp square_bread -->
      <Action ID="GRASP" obj="square_bread" />
      <!-- Navigate to blue_bowl -->
      <Action ID="NAVIGATE_TO" obj="blue_bowl" />
      <!-- Place inside blue_bowl -->
      <Action ID="PLACE_INSIDE" obj="blue_bowl" />
    </Sequence>
  </BehaviorTree>
</root>
