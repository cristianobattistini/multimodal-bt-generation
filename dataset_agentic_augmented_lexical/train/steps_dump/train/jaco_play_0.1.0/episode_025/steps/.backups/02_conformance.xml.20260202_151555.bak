<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to milk_dairy -->
      <Action ID="NAVIGATE_TO" obj="milk_dairy" />
      <!-- Grasp milk_dairy -->
      <Action ID="GRASP" obj="milk_dairy" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Check table is holding before PLACE_ON_TOP -->
      <Condition ID="IS_HOLDING" obj="table" />
      <!-- Fallback: if PLACE_ON_TOP table fails, try alternative to reposition -->
      <Fallback>
        <!-- Place on table -->
        <Action ID="PLACE_ON_TOP" obj="table" />
        <Sequence>
          <!-- Push table forward toward front edge of table -->
          <Action ID="PUSH" obj="table" />
          <!-- Place on table -->
          <Action ID="PLACE_ON_TOP" obj="table" />
        </Sequence>
      </Fallback>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
