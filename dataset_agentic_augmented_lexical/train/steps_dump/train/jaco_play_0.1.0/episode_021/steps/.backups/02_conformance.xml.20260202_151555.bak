<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to gray_plate -->
      <Action ID="NAVIGATE_TO" obj="gray_plate" />
      <!-- Retry PLACE_INSIDE gray_plate up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Place inside gray_plate -->
        <Action ID="PLACE_INSIDE" obj="gray_plate" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
