<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to apple_fruit -->
      <Action ID="NAVIGATE_TO" obj="apple_fruit" />
      <!-- Grasp apple_fruit -->
      <Action ID="GRASP" obj="apple_fruit" />
      <!-- Timeout: NAVIGATE_TO sink within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to sink -->
        <Action ID="NAVIGATE_TO" obj="sink" />
      </Timeout>
      <!-- Place inside sink -->
      <Action ID="PLACE_INSIDE" obj="sink" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
