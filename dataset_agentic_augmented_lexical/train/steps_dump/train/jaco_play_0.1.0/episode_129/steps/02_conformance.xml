<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach blue_bowl -->
      <Action ID="APPROACH" obj="blue_bowl" />
      <!-- Retry GRASP milk_dairy up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Grasp milk_dairy -->
        <Action ID="GRASP" obj="milk_dairy" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
