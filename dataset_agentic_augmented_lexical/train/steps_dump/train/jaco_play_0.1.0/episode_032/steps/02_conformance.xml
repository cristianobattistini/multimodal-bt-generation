<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to black_bowl -->
      <Action ID="NAVIGATE_TO" obj="black_bowl" />
      <!-- Grasp black_bowl -->
      <Action ID="GRASP" obj="black_bowl" />
      <!-- Timeout: NAVIGATE_TO gray_plate within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to gray_plate -->
        <Action ID="NAVIGATE_TO" obj="gray_plate" />
      </Timeout>
      <!-- Place inside gray_plate -->
      <Action ID="PLACE_INSIDE" obj="gray_plate" />
    </Sequence>
  </BehaviorTree>
</root>
