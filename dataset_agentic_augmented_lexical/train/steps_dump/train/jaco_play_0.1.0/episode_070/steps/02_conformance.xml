<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <Sequence>
        <!-- Check steak_meat is reachable before NAVIGATE_TO -->
        <Condition ID="IS_REACHABLE" obj="steak_meat" />
        <!-- Navigate to steak_meat -->
        <Action ID="NAVIGATE_TO" obj="steak_meat" />
      </Sequence>
      <!-- Grasp steak_meat -->
      <Action ID="GRAB" obj="steak_meat" />
    </Sequence>
  </BehaviorTree>
</root>
