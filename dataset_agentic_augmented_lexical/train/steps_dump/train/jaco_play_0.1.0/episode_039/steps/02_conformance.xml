<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to steak_meat -->
      <Action ID="NAVIGATE_TO" obj="steak_meat" />
      <!-- Fallback: if GRASP steak_meat fails, try PUSH, FLIP to reposition -->
      <Fallback>
        <!-- Grasp steak_meat -->
        <Action ID="GRASP" obj="steak_meat" />
        <Sequence>
          <!-- Push steak_meat forward toward front edge of table -->
          <Action ID="PUSH" obj="steak_meat" />
          <!-- Grasp steak_meat -->
          <Action ID="GRASP" obj="steak_meat" />
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
