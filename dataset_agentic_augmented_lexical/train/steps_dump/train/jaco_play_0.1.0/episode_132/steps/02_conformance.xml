<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to square_bread -->
      <Action ID="NAVIGATE_TO" obj="square_bread" />
      <!-- Retry GRASP square_bread up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Timeout: GRASP square_bread within 10s -->
        <Timeout msec="10000">
          <!-- Grasp square_bread -->
          <Action ID="GRASP" obj="square_bread" />
        </Timeout>
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
