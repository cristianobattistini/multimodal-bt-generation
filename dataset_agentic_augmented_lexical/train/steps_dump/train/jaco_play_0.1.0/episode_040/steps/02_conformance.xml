<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to long_bread -->
      <Action ID="NAVIGATE_TO" obj="long_bread" />
      <!-- Grasp long_bread -->
      <Action ID="GRASP" obj="long_bread" />
      <!-- Navigate to gray_bowl -->
      <Action ID="NAVIGATE_TO" obj="gray_bowl" />
      <!-- Check gray_bowl is holding before PLACE_INSIDE -->
      <Condition ID="IS_HOLDING" obj="gray_bowl" />
      <!-- Retry PLACE_INSIDE gray_bowl up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Place inside gray_bowl -->
        <Action ID="PLACE_INSIDE" obj="gray_bowl" />
      </RetryUntilSuccessful>
      
    </Sequence>
  </BehaviorTree>
</root>
