<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to fish -->
      <Action ID="NAVIGATE_TO" obj="fish" />
      <!-- Grasp fish -->
      <Action ID="GRASP" obj="fish" />
      <!-- Navigate to frying_pan -->
      <Action ID="NAVIGATE_TO" obj="frying_pan" />
      <Sequence>
        <!-- Check frying_pan is holding before PLACE_INSIDE -->
        <Condition ID="IS_HOLDING" obj="frying_pan" />
        <!-- Place inside frying_pan -->
        <Action ID="PLACE_INSIDE" obj="frying_pan" />
      </Sequence>
      
      <!-- Navigate to sausage -->
      <Action ID="NAVIGATE_TO" obj="sausage" />
      <!-- Grasp sausage -->
      <Action ID="GRASP" obj="sausage" />
      <!-- Navigate to frying_pan -->
      <Action ID="NAVIGATE_TO" obj="frying_pan" />
      <Sequence>
        <!-- Check frying_pan is holding before PLACE_INSIDE -->
        <Condition ID="IS_HOLDING" obj="frying_pan" />
        <!-- Place inside frying_pan -->
        <Action ID="PLACE_INSIDE" obj="frying_pan" />
      </Sequence>
      
      <!-- Navigate to tomato -->
      <Action ID="NAVIGATE_TO" obj="tomato" />
      <!-- Grasp tomato -->
      <Action ID="GRASP" obj="tomato" />
      <!-- Navigate to frying_pan -->
      <Action ID="NAVIGATE_TO" obj="frying_pan" />
      <Sequence>
        <!-- Check frying_pan is holding before PLACE_INSIDE -->
        <Condition ID="IS_HOLDING" obj="frying_pan" />
        <!-- Place inside frying_pan -->
        <Action ID="PLACE_INSIDE" obj="frying_pan" />
      </Sequence>
      
    </Sequence>
  </BehaviorTree>
</root>
