<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check pan is reachable before NAVIGATE_TO -->
      <Condition ID="IS_REACHABLE" obj="pan" />
      <!-- Timeout: NAVIGATE_TO pan within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to pan -->
        <Action ID="NAVIGATE_TO" obj="pan" />
      </Timeout>
      <!-- Grasp pan -->
      <Action ID="GRASP" obj="pan" />
      <!-- Navigate to stove -->
      <Action ID="NAVIGATE_TO" obj="stove" />
      <!-- Place on stove -->
      <Action ID="PLACE_ON_TOP" obj="stove" />
      <!-- Release -->
      <Action ID="RELEASE" />
      <!-- Navigate to fish -->
      <Action ID="NAVIGATE_TO" obj="fish" />
      <!-- Grasp fish -->
      <Action ID="GRASP" obj="fish" />
      <!-- Check pan is reachable before NAVIGATE_TO -->
      <Condition ID="IS_REACHABLE" obj="pan" />
      <!-- Timeout: NAVIGATE_TO pan within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to pan -->
        <Action ID="NAVIGATE_TO" obj="pan" />
      </Timeout>
      <!-- Place inside pan -->
      <Action ID="PLACE_INSIDE" obj="pan" />
      <!-- Release -->
      <Action ID="RELEASE" />
      <!-- Navigate to sausage -->
      <Action ID="NAVIGATE_TO" obj="sausage" />
      <!-- Grasp sausage -->
      <Action ID="GRASP" obj="sausage" />
      <!-- Check pan is reachable before NAVIGATE_TO -->
      <Condition ID="IS_REACHABLE" obj="pan" />
      <!-- Timeout: NAVIGATE_TO pan within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to pan -->
        <Action ID="NAVIGATE_TO" obj="pan" />
      </Timeout>
      <!-- Place inside pan -->
      <Action ID="PLACE_INSIDE" obj="pan" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
