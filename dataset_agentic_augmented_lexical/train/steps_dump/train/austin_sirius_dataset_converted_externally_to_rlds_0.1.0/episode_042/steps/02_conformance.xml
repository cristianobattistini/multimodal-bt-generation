<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Approach kcup_holder -->
      <Action ID="APPROACH" obj="kcup_holder" />
      <!-- Open kcup_holder -->
      <Action ID="OPEN" obj="kcup_holder" />
      <!-- Navigate to kcup -->
      <Action ID="NAVIGATE_TO" obj="kcup" />
      <!-- Retry GRASP kcup up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Grasp kcup -->
        <Action ID="GRASP" obj="kcup" />
      </RetryUntilSuccessful>
      <!-- Navigate to kcup_holder -->
      <Action ID="MOVE_TO" obj="kcup_holder" />
      <!-- Move to kcup_holder -->
      <Action ID="PUT_IN" obj="kcup_holder" />
      <!-- Put in kcup_holder -->
      <Action ID="SEAL" obj="kcup_holder" />
    </Sequence>
  </BehaviorTree>
</root>
