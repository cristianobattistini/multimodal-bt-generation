<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO kcup_holder within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to kcup_holder -->
        <Action ID="MOVE_TO" obj="kcup_holder" />
      </Timeout>
      <!-- Move to kcup_holder -->
      <Action ID="UNSEAL" obj="kcup_holder" />
      <!-- Unseal kcup_holder -->
      <Action ID="NAVIGATE_TO" obj="kcup" />
      <!-- Grasp kcup -->
      <Action ID="GRASP" obj="kcup" />
      <!-- Timeout: NAVIGATE_TO kcup_holder within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to kcup_holder -->
        <Action ID="NAVIGATE_TO" obj="kcup_holder" />
      </Timeout>
      <!-- Place inside kcup_holder -->
      <Action ID="INSERT_INTO" obj="kcup_holder" />
      <!-- Timeout: NAVIGATE_TO kcup_holder within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to kcup_holder -->
        <Action ID="LOCOMOTE_TO" obj="kcup_holder" />
      </Timeout>
      <!-- Close kcup_holder -->
      <Action ID="CLOSE" obj="kcup_holder" />
    </Sequence>
  </BehaviorTree>
</root>
