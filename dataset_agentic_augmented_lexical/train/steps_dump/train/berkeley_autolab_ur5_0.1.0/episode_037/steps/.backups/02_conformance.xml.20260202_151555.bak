<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_cup -->
      <Action ID="NAVIGATE_TO" obj="blue_cup" />
      <!-- Check blue_cup is reachable before GRASP -->
      <Condition ID="IS_REACHABLE" obj="blue_cup" />
      <!-- Retry GRASP blue_cup up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Timeout: GRASP blue_cup within 15s -->
        <Timeout msec="15000">
          <!-- Grasp blue_cup -->
          <Action ID="GRASP" obj="blue_cup" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Navigate to brown_cup -->
      <Action ID="NAVIGATE_TO" obj="brown_cup" />
      <!-- Place inside brown_cup -->
      <Action ID="PLACE_INSIDE" obj="brown_cup" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
