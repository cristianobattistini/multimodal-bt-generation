<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to blue_cup -->
      <Action ID="MOVE_TO" obj="blue_cup" />
      <!-- Grasp blue_cup -->
      <Action ID="GRASP" obj="blue_cup" />
      <!-- Timeout: NAVIGATE_TO brown_cup within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to brown_cup -->
        <Action ID="NAVIGATE_TO" obj="brown_cup" />
      </Timeout>
      <!-- Place inside brown_cup -->
      <Action ID="SET_IN" obj="brown_cup" />
    </Sequence>
  </BehaviorTree>
</root>
