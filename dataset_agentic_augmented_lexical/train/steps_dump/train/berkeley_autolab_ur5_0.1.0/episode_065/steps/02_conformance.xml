<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachRanchBottle -->
      <SubTree ID="ApproachRanchBottle" />
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Insert into pot -->
      <Action ID="INSERT_INTO" obj="pot" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachRanchBottle -->
  <BehaviorTree ID="ApproachRanchBottle">
    <Sequence>
      <!-- Navigate to ranch_bottle -->
      <Action ID="NAVIGATE_TO" obj="ranch_bottle" />
      <!-- Grasp ranch_bottle -->
      <Action ID="GRASP" obj="ranch_bottle" />
    </Sequence>
  </BehaviorTree>
</root>
