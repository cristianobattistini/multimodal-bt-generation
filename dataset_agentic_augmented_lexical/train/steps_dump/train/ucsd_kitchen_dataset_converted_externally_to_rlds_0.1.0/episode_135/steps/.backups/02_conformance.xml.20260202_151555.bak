<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to green_box -->
      <Action ID="NAVIGATE_TO" obj="green_box" />
      <!-- Grasp green_box -->
      <Action ID="GRASP" obj="green_box" />
      <!-- Navigate to sink -->
      <Action ID="NAVIGATE_TO" obj="sink" />
      <Sequence>
        <!-- Check sink is open before PLACE_INSIDE -->
        <Condition ID="IS_OPEN" obj="sink" />
        <!-- Place inside sink -->
        <Action ID="PLACE_INSIDE" obj="sink" />
      </Sequence>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
