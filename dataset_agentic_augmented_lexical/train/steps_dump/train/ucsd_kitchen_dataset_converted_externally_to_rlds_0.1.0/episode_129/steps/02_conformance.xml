<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO white_box within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to white_box -->
        <Action ID="GO_TO" obj="white_box" />
      </Timeout>
      <!-- Go to white_box -->
      <Action ID="GRAB" obj="white_box" />
      <!-- Grab white_box -->
      <Action ID="GO_TO" obj="sink" />
      <!-- Go to sink -->
      <Action ID="PLACE_INSIDE" obj="sink" />
    </Sequence>
  </BehaviorTree>
</root>
