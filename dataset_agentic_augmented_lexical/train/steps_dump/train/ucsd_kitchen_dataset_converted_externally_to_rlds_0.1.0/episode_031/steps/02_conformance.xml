<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to canned_spam -->
      <Action ID="NAVIGATE_TO" obj="canned_spam" />
      <Sequence>
        <!-- Check canned_spam is visible before GRASP -->
        <Condition ID="IS_VISIBLE" obj="canned_spam" />
        <!-- Grasp canned_spam -->
        <Action ID="GRASP" obj="canned_spam" />
      </Sequence>
      <!-- Navigate to sink -->
      <Action ID="NAVIGATE_TO" obj="sink" />
      <!-- Place inside sink -->
      <Action ID="PLACE_INSIDE" obj="sink" />
    </Sequence>
  </BehaviorTree>
</root>
