<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to drying_mat -->
      <Action ID="NAVIGATE_TO" obj="drying_mat" />
      <!-- Grasp plate -->
      <Action ID="GRASP" obj="plate" />
      <!-- Navigate to dish_rack -->
      <Action ID="NAVIGATE_TO" obj="dish_rack" />
      <Sequence>
        <!-- Check dish_rack is empty before PLACE_INSIDE -->
        <Condition ID="IS_EMPTY" obj="dish_rack" />
        <!-- Place inside dish_rack -->
        <Action ID="PLACE_INSIDE" obj="dish_rack" />
      </Sequence>
      <!-- Release -->
      <Action ID="RELEASE" />
      <!-- Navigate to drying_mat -->
      <Action ID="NAVIGATE_TO" obj="drying_mat" />
      <!-- Grasp cup -->
      <Action ID="GRASP" obj="cup" />
      <!-- Navigate to dish_rack -->
      <Action ID="NAVIGATE_TO" obj="dish_rack" />
      <Sequence>
        <!-- Check dish_rack is empty before PLACE_INSIDE -->
        <Condition ID="IS_EMPTY" obj="dish_rack" />
        <!-- Place inside dish_rack -->
        <Action ID="PLACE_INSIDE" obj="dish_rack" />
      </Sequence>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
