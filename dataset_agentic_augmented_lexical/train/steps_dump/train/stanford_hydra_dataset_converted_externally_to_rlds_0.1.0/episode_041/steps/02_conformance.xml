<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachYellowPlate -->
      <SubTree ID="ApproachYellowPlate" />
      <!-- Move to dish_rack -->
      <Action ID="MOVE_TO" obj="dish_rack" />
      <!-- Put in dish_rack -->
      <Action ID="PUT_IN" obj="dish_rack" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachYellowPlate -->
  <BehaviorTree ID="ApproachYellowPlate">
    <Sequence>
      <!-- Move to yellow_plate -->
      <Action ID="MOVE_TO" obj="yellow_plate" />
      <!-- Pick yellow_plate -->
      <Action ID="PICK" obj="yellow_plate" />
    </Sequence>
  </BehaviorTree>
</root>
