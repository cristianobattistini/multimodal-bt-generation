<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to towel -->
      <Action ID="NAVIGATE_TO" obj="towel" />
      <!-- Retry GRASP towel up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Grasp towel -->
        <Action ID="GRASP" obj="towel" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
