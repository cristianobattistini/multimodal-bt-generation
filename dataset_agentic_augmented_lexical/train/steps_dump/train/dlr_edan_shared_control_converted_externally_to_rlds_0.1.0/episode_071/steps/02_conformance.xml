<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO black_shelving_unit within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to black_shelving_unit -->
        <Action ID="NAVIGATE_TO" obj="black_shelving_unit" />
      </Timeout>
      <!-- Navigate to tube -->
      <Action ID="NAVIGATE_TO" obj="tube" />
      <!-- Grasp tube -->
      <Action ID="GRASP" obj="tube" />
    </Sequence>
  </BehaviorTree>
</root>
