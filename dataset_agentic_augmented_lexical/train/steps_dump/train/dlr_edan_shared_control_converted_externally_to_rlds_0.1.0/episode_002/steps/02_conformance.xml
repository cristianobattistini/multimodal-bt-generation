<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check small_bowl is reachable before NAVIGATE_TO -->
      <Condition ID="IS_REACHABLE" obj="small_bowl" />
      <!-- Timeout: NAVIGATE_TO small_bowl within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to small_bowl -->
        <Action ID="NAVIGATE_TO" obj="small_bowl" />
      </Timeout>
      <!-- Grasp tennis_ball -->
      <Action ID="GRASP" obj="tennis_ball" />
    </Sequence>
  </BehaviorTree>
</root>
