<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check small_round_side_table is visible before NAVIGATE_TO -->
      <Condition ID="IS_VISIBLE" obj="small_round_side_table" />
      <!-- Timeout: NAVIGATE_TO small_round_side_table within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to small_round_side_table -->
        <Action ID="NAVIGATE_TO" obj="small_round_side_table" />
      </Timeout>
      <!-- Navigate to green_apple -->
      <Action ID="NAVIGATE_TO" obj="green_apple" />
      <!-- Grasp green_apple -->
      <Action ID="GRASP" obj="green_apple" />
    </Sequence>
  </BehaviorTree>
</root>
