<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to black_shelving_unit -->
      <Action ID="NAVIGATE_TO" obj="black_shelving_unit" />
      <!-- Navigate to cube -->
      <Action ID="NAVIGATE_TO" obj="cube" />
      <!-- Retry GRASP cube up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Grasp cube -->
        <Action ID="GRASP" obj="cube" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
