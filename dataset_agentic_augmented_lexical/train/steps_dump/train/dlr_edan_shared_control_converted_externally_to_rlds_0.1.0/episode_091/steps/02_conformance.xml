<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Go to black_cabinet -->
      <Action ID="GO_TO" obj="black_cabinet" />
      <!-- Approach disk -->
      <Action ID="APPROACH" obj="disk" />
      <!-- Retry GRASP disk up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Grasp disk -->
        <Action ID="GRIP" obj="disk" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
