<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to drawer -->
      <Action ID="NAVIGATE_TO" obj="drawer" />
      <!-- Retry OPEN drawer up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Open drawer -->
        <Action ID="OPEN" obj="drawer" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
