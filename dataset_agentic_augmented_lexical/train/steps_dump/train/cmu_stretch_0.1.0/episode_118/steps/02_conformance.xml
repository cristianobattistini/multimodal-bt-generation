<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to cabinet_right_door -->
      <Action ID="NAVIGATE_TO" obj="cabinet_right_door" />
      <Sequence>
        <!-- Check cabinet_right_door is closed before OPEN -->
        <Condition ID="IS_CLOSED" obj="cabinet_right_door" />
        <!-- Open cabinet_right_door -->
        <Action ID="OPEN" obj="cabinet_right_door" />
      </Sequence>
    </Sequence>
  </BehaviorTree>
</root>
