        <Timeout msec="15000">
          <!-- Grasp green_garbage_can_lid -->
          <Action ID="GRASP" obj="green_garbage_can_lid" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Open green_garbage_can_lid -->
      <Action ID="OPEN" obj="green_garbage_can_lid" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
