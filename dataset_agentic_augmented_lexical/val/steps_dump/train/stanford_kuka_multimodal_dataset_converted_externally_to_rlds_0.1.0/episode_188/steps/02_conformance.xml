<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to peg -->
      <Action ID="NAVIGATE_TO" obj="peg" />
      <!-- Fallback: if GRASP peg fails, try PUSH, FLIP to reposition -->
      <Fallback>
        <!-- Grasp peg -->
        <Action ID="GRASP" obj="peg" />
        <Sequence>
          <!-- Push peg forward toward front edge of table -->
          <Action ID="PUSH" obj="peg" />
          <!-- Grasp peg -->
          <Action ID="GRASP" obj="peg" />
        </Sequence>
      </Fallback>
      <!-- Navigate to hole -->
      <Action ID="NAVIGATE_TO" obj="hole" />
      <!-- Place inside hole -->
      <Action ID="PLACE_INSIDE" obj="hole" />
    </Sequence>
  </BehaviorTree>
</root>
