<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO hole within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to hole -->
        <Action ID="NAVIGATE_TO" obj="hole" />
      </Timeout>
      <!-- Place inside hole -->
      <Action ID="PLACE_INSIDE" obj="hole" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
