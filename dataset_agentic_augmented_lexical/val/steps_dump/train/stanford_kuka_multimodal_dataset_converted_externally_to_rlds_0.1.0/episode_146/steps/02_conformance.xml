<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to hole -->
      <Action ID="NAVIGATE_TO" obj="hole" />
      <!-- Fallback: if PLACE_INSIDE hole fails, try PUSH to reposition -->
      <Fallback>
        <!-- Place inside hole -->
        <Action ID="PLACE_INSIDE" obj="hole" />
        <Sequence>
          <!-- Push hole forward toward front edge of table -->
          <Action ID="PUSH" obj="hole" />
          <!-- Place inside hole -->
          <Action ID="PLACE_INSIDE" obj="hole" />
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
