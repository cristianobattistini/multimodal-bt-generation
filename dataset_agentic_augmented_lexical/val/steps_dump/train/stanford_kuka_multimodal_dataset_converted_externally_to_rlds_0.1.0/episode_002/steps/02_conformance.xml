<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Fallback: if PLACE_INSIDE hole fails, try alternative to reposition -->
      <Fallback>
        <!-- Retry PLACE_INSIDE hole up to 2 attempts -->
        <RetryUntilSuccessful num_attempts="2">
          <!-- Place inside hole -->
          <Action ID="PLACE_INSIDE" obj="hole" />
        </RetryUntilSuccessful>
        <Sequence>
          <!-- Push hole forward toward front edge of table -->
          <Action ID="SHOVE" obj="hole" />
          <!-- Place inside hole -->
          <Action ID="SET_IN" obj="hole" />
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
