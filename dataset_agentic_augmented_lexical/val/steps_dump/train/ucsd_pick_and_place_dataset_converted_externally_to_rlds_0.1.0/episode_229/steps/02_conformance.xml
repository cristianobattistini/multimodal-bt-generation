<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Move to green_object -->
      <Action ID="MOVE_TO" obj="green_object" />
      <!-- Fallback: if GRASP green_object fails, try PUSH, FLIP to reposition -->
      <Fallback>
        <!-- Grasp green_object -->
        <Action ID="GRAB" obj="green_object" />
        <Sequence>
          <!-- Push green_object forward toward front edge of table -->
          <Action ID="PUSH" obj="green_object" />
          <!-- Grasp green_object -->
          <Action ID="PICK" obj="green_object" />
        </Sequence>
      </Fallback>
    </Sequence>
  </BehaviorTree>
</root>
