<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO red_object within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to red_object -->
        <Action ID="MOVE_TO" obj="red_object" />
      </Timeout>
      <!-- Move to red_object -->
      <Action ID="PICK" obj="red_object" />
    </Sequence>
  </BehaviorTree>
</root>
