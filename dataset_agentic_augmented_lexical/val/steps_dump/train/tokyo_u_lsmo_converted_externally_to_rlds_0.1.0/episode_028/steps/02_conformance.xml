<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Timeout: NAVIGATE_TO blue_pen within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to blue_pen -->
        <Action ID="NAVIGATE_TO" obj="blue_pen" />
      </Timeout>
      <!-- Grasp blue_pen -->
      <Action ID="GRASP" obj="blue_pen" />
    </Sequence>
  </BehaviorTree>
</root>
