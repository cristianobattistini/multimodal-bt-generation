<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Navigate to scissors -->
      <Action ID="NAVIGATE_TO" obj="scissors" />
      <Sequence>
        <!-- Check scissors is graspable before GRASP -->
        <Condition ID="IS_GRASPABLE" obj="scissors" />
        <!-- Grasp scissors -->
        <Action ID="GRASP" obj="scissors" />
      </Sequence>
    </Sequence>
  </BehaviorTree>
</root>
