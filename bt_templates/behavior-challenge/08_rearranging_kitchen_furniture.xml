<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Open cabinet -->
      <Action ID="NAVIGATE_TO" obj="cabinet.n.01_1"/>
      <Action ID="OPEN" obj="cabinet.n.01_1"/>
      <!-- Store toaster -->
      <Action ID="NAVIGATE_TO" obj="toaster.n.02_1"/>
      <Action ID="GRASP" obj="toaster.n.02_1"/>
      <Action ID="NAVIGATE_TO" obj="cabinet.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="cabinet.n.01_1"/>
      <!-- Store food processor -->
      <Action ID="NAVIGATE_TO" obj="food_processor.n.01_1"/>
      <Action ID="GRASP" obj="food_processor.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="cabinet.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="cabinet.n.01_1"/>
      <!-- Store french press -->
      <Action ID="NAVIGATE_TO" obj="french_press.n.01_1"/>
      <Action ID="GRASP" obj="french_press.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="cabinet.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="cabinet.n.01_1"/>
      <!-- Close cabinet -->
      <Action ID="CLOSE" obj="cabinet.n.01_1"/>
    </Sequence>
  </BehaviorTree>
</root>
