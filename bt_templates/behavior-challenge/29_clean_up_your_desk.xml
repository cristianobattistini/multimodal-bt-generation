<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Put folders in bookcase -->
      <Action ID="NAVIGATE_TO" obj="folder.n.02_1"/>
      <Action ID="GRASP" obj="folder.n.02_1"/>
      <Action ID="NAVIGATE_TO" obj="bookcase.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="bookcase.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="folder.n.02_2"/>
      <Action ID="GRASP" obj="folder.n.02_2"/>
      <Action ID="NAVIGATE_TO" obj="bookcase.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="bookcase.n.01_1"/>
      <!-- Put paperback books in bookcase -->
      <Action ID="NAVIGATE_TO" obj="paperback_book.n.01_1"/>
      <Action ID="GRASP" obj="paperback_book.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="bookcase.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="bookcase.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="paperback_book.n.01_2"/>
      <Action ID="GRASP" obj="paperback_book.n.01_2"/>
      <Action ID="NAVIGATE_TO" obj="bookcase.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="bookcase.n.01_1"/>
      <!-- Put writing tools in pencil box -->
      <Action ID="NAVIGATE_TO" obj="pencil.n.01_1"/>
      <Action ID="GRASP" obj="pencil.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="pencil_box.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="pencil_box.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="pen.n.01_1"/>
      <Action ID="GRASP" obj="pen.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="pencil_box.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="pencil_box.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="pen.n.01_2"/>
      <Action ID="GRASP" obj="pen.n.01_2"/>
      <Action ID="NAVIGATE_TO" obj="pencil_box.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="pencil_box.n.01_1"/>
      <!-- Take stapler from bookcase to desk -->
      <Action ID="NAVIGATE_TO" obj="stapler.n.01_1"/>
      <Action ID="GRASP" obj="stapler.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="desk.n.01_1"/>
      <Action ID="PLACE_ON_TOP" obj="desk.n.01_1"/>
      <!-- Move laptop from bed to desk and close it -->
      <Action ID="NAVIGATE_TO" obj="laptop.n.01_1"/>
      <Action ID="GRASP" obj="laptop.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="desk.n.01_1"/>
      <Action ID="PLACE_ON_TOP" obj="desk.n.01_1"/>
      <Action ID="CLOSE" obj="laptop.n.01_1"/>
    </Sequence>
  </BehaviorTree>
</root>
