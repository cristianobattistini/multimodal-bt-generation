<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Open toolbox first -->
      <Action ID="NAVIGATE_TO" obj="toolbox.n.01_1"/>
      <Action ID="OPEN" obj="toolbox.n.01_1"/>
      <!-- Place drill inside toolbox -->
      <Action ID="NAVIGATE_TO" obj="drill.n.01_1"/>
      <Action ID="GRASP" obj="drill.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="toolbox.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="toolbox.n.01_1"/>
      <!-- Place pliers inside toolbox -->
      <Action ID="NAVIGATE_TO" obj="pliers.n.01_1"/>
      <Action ID="GRASP" obj="pliers.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="toolbox.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="toolbox.n.01_1"/>
      <!-- Place flashlight inside toolbox -->
      <Action ID="NAVIGATE_TO" obj="flashlight.n.01_1"/>
      <Action ID="GRASP" obj="flashlight.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="toolbox.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="toolbox.n.01_1"/>
      <!-- Place allen_wrench inside toolbox -->
      <Action ID="NAVIGATE_TO" obj="allen_wrench.n.01_1"/>
      <Action ID="GRASP" obj="allen_wrench.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="toolbox.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="toolbox.n.01_1"/>
      <!-- Place screwdriver inside toolbox -->
      <Action ID="NAVIGATE_TO" obj="screwdriver.n.01_1"/>
      <Action ID="GRASP" obj="screwdriver.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="toolbox.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="toolbox.n.01_1"/>
      <!-- Close toolbox -->
      <Action ID="CLOSE" obj="toolbox.n.01_1"/>
    </Sequence>
  </BehaviorTree>
</root>
