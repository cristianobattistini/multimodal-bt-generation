<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Put camera in container -->
      <Action ID="NAVIGATE_TO" obj="digital_camera.n.01_1"/>
      <Action ID="GRASP" obj="digital_camera.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="container.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="container.n.01_1"/>
      <!-- Open car trunk -->
      <Action ID="NAVIGATE_TO" obj="car.n.01_1"/>
      <Action ID="OPEN" obj="car.n.01_1"/>
      <!-- Load container into car -->
      <Action ID="NAVIGATE_TO" obj="container.n.01_1"/>
      <Action ID="GRASP" obj="container.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="car.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="car.n.01_1"/>
      <!-- Load tennis racket into car -->
      <Action ID="NAVIGATE_TO" obj="tennis_racket.n.01_1"/>
      <Action ID="GRASP" obj="tennis_racket.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="car.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="car.n.01_1"/>
      <!-- Close car trunk -->
      <Action ID="CLOSE" obj="car.n.01_1"/>
    </Sequence>
  </BehaviorTree>
</root>
