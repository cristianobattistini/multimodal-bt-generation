      <Action ID="GRASP" obj="bottle__of__detergent.n.01_2"/>
      <Action ID="NAVIGATE_TO" obj="bottle__of__detergent.n.01_1"/>
      <Action ID="PLACE_NEXT_TO" obj="bottle__of__detergent.n.01_1"/>
      <!-- Sanitary napkin box on shelf -->
      <Action ID="NAVIGATE_TO" obj="box__of__sanitary_napkin.n.01_1"/>
