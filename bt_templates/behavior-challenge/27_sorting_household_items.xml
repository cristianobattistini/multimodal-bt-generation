      <Action ID="PLACE_NEXT_TO" obj="cup.n.01_1"/>
      <!-- Toothbrush inside cup -->
      <Action ID="NAVIGATE_TO" obj="toothbrush.n.01_1"/>
      <Action ID="GRASP" obj="toothbrush.n.01_1"/>
      <Action ID="NAVIGATE_TO" obj="cup.n.01_1"/>
      <Action ID="PLACE_INSIDE" obj="cup.n.01_1"/>
