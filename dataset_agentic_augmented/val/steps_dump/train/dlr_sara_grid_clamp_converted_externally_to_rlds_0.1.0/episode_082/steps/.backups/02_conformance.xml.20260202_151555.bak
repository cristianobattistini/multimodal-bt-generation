<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp" />
      <!-- Retry GRASP grid_clamp up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Timeout: GRASP grid_clamp within 10s -->
        <Timeout msec="10000">
          <!-- Grasp grid_clamp -->
          <Action ID="GRASP" obj="grid_clamp" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Navigate to grid_board -->
      <Action ID="NAVIGATE_TO" obj="grid_board" />
      <!-- Place on grid_board -->
      <Action ID="PLACE_ON_TOP" obj="grid_board" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
