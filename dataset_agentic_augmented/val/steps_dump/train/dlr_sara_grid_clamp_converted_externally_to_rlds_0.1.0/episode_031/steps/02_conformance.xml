<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachGridClamp -->
      <SubTree ID="ApproachGridClamp" />
      <!-- Navigate to grid_board -->
      <Action ID="NAVIGATE_TO" obj="grid_board" />
      <!-- Place inside grid_board -->
      <Action ID="PLACE_INSIDE" obj="grid_board" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachGridClamp -->
  <BehaviorTree ID="ApproachGridClamp">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp" />
      <!-- Grasp grid_clamp -->
      <Action ID="GRASP" obj="grid_clamp" />
    </Sequence>
  </BehaviorTree>
</root>
