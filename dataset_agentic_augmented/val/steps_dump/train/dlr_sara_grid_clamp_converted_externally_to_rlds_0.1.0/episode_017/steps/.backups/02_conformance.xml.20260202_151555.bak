<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to grid_clamp -->
      <Action ID="NAVIGATE_TO" obj="grid_clamp" />
      <!-- Retry GRASP grid_clamp up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Timeout: GRASP grid_clamp within 15s -->
        <Timeout msec="15000">
          <!-- Grasp grid_clamp -->
          <Action ID="GRASP" obj="grid_clamp" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Navigate to grid_plate -->
      <Action ID="NAVIGATE_TO" obj="grid_plate" />
      <!-- Place inside grid_plate -->
      <Action ID="PLACE_INSIDE" obj="grid_plate" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
