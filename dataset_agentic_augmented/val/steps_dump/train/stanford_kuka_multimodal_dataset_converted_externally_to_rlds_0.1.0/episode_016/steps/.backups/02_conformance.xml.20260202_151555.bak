<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Retry PLACE_INSIDE hole up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Place inside hole -->
        <Action ID="PLACE_INSIDE" obj="hole" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
