<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO orange within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to orange -->
        <Action ID="NAVIGATE_TO" obj="orange" />
      </Timeout>
      <!-- Grasp orange -->
      <Action ID="GRASP" obj="orange" />
    </Sequence>
  </BehaviorTree>
</root>
