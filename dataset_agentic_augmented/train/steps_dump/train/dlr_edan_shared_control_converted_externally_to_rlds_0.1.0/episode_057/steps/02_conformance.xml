<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <Sequence>
        <!-- Check black_shelf is reachable before NAVIGATE_TO -->
        <Condition ID="IS_REACHABLE" obj="black_shelf" />
        <!-- Navigate to black_shelf -->
        <Action ID="NAVIGATE_TO" obj="black_shelf" />
      </Sequence>
      <!-- Navigate to cube -->
      <Action ID="NAVIGATE_TO" obj="cube" />
      <!-- Grasp cube -->
      <Action ID="GRASP" obj="cube" />
    </Sequence>
  </BehaviorTree>
</root>
