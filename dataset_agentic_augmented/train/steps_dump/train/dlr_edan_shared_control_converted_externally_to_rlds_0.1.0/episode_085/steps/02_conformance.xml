<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO red_apple within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to red_apple -->
        <Action ID="NAVIGATE_TO" obj="red_apple" />
      </Timeout>
      <!-- Grasp red_apple -->
      <Action ID="GRASP" obj="red_apple" />
    </Sequence>
  </BehaviorTree>
</root>
