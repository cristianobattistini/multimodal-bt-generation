<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO small_round_stool within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to small_round_stool -->
        <Action ID="NAVIGATE_TO" obj="small_round_stool" />
      </Timeout>
      <!-- Navigate to yellow_ball -->
      <Action ID="NAVIGATE_TO" obj="yellow_ball" />
      <!-- Grasp yellow_ball -->
      <Action ID="GRASP" obj="yellow_ball" />
    </Sequence>
  </BehaviorTree>
</root>
