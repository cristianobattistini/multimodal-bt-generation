<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO shelf_compartment within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to shelf_compartment -->
        <Action ID="NAVIGATE_TO" obj="shelf_compartment" />
      </Timeout>
      <!-- Navigate to green_apple -->
      <Action ID="NAVIGATE_TO" obj="green_apple" />
      <!-- Grasp green_apple -->
      <Action ID="GRASP" obj="green_apple" />
    </Sequence>
  </BehaviorTree>
</root>
