<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to green_cloth -->
      <Action ID="NAVIGATE_TO" obj="green_cloth" />
      <Sequence>
        <!-- Check green_cloth is visible before PUSH -->
        <Condition ID="IS_VISIBLE" obj="green_cloth" />
        <!-- Push green_cloth forward toward front edge of table -->
        <Action ID="PUSH" obj="green_cloth" />
      </Sequence>
    </Sequence>
  </BehaviorTree>
</root>
