<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_cup -->
      <Action ID="NAVIGATE_TO" obj="blue_cup" />
      <Sequence>
        <!-- Check blue_cup is visible before GRASP -->
        <Condition ID="IS_VISIBLE" obj="blue_cup" />
        <!-- Grasp blue_cup -->
        <Action ID="GRASP" obj="blue_cup" />
      </Sequence>
      <!-- Navigate to brown_cup -->
      <Action ID="NAVIGATE_TO" obj="brown_cup" />
      <!-- Place inside brown_cup -->
      <Action ID="PLACE_INSIDE" obj="brown_cup" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
