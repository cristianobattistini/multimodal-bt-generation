<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to top_drawer -->
      <Action ID="NAVIGATE_TO" obj="top_drawer" />
      <!-- Retry OPEN top_drawer up to 4 attempts -->
      <RetryUntilSuccessful num_attempts="4">
        <!-- Open top_drawer -->
        <Action ID="OPEN" obj="top_drawer" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
