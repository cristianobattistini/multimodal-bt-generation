      <!-- Navigate to green_garbage_can_lid -->
      <Action ID="NAVIGATE_TO" obj="green_garbage_can_lid" />
      <!-- Grasp green_garbage_can_lid -->
      <Action ID="GRASP" obj="green_garbage_can_lid" />
      <!-- Retry OPEN green_garbage_can_lid up to 4 attempts -->
