<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to bottom_drawer -->
      <Action ID="NAVIGATE_TO" obj="bottom_drawer" />
      <!-- Navigate to rxbar_chocolate -->
      <Action ID="NAVIGATE_TO" obj="rxbar_chocolate" />
      <!-- Fallback: if GRASP rxbar_chocolate fails, try PUSH, FLIP to reposition -->
      <Fallback>
        <!-- Grasp rxbar_chocolate -->
        <Action ID="GRASP" obj="rxbar_chocolate" />
        <Sequence>
          <!-- Push rxbar_chocolate forward toward front edge of table -->
          <Action ID="PUSH" obj="rxbar_chocolate" />
          <!-- Grasp rxbar_chocolate -->
          <Action ID="GRASP" obj="rxbar_chocolate" />
        </Sequence>
      </Fallback>
      <!-- Navigate to counter -->
      <Action ID="NAVIGATE_TO" obj="counter" />
      <!-- Place on counter -->
      <Action ID="PLACE_ON_TOP" obj="counter" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
