<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to coke_can -->
      <Action ID="NAVIGATE_TO" obj="coke_can" />
      <Sequence>
        <!-- Check coke_can is reachable before GRASP -->
        <Condition ID="IS_REACHABLE" obj="coke_can" />
        <!-- Retry GRASP coke_can up to 3 attempts -->
        <RetryUntilSuccessful num_attempts="3">
          <!-- Timeout: GRASP coke_can within 10s -->
          <Timeout msec="10000">
            <!-- Grasp coke_can -->
            <Action ID="GRASP" obj="coke_can" />
          </Timeout>
        </RetryUntilSuccessful>
      </Sequence>
      <!-- Navigate to middle_drawer -->
      <Action ID="NAVIGATE_TO" obj="middle_drawer" />
      <!-- Place inside middle_drawer -->
      <Action ID="PLACE_INSIDE" obj="middle_drawer" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
