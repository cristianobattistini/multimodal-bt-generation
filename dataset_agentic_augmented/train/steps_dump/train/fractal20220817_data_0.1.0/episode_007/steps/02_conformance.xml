<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to middle_drawer -->
      <Action ID="NAVIGATE_TO" obj="middle_drawer" />
      <!-- Retry OPEN middle_drawer up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Open middle_drawer -->
        <Action ID="OPEN" obj="middle_drawer" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
