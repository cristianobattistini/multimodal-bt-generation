<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to wrinkled_towel -->
      <Action ID="NAVIGATE_TO" obj="wrinkled_towel" />
      <!-- Retry GRASP wrinkled_towel up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Timeout: GRASP wrinkled_towel within 10s -->
        <Timeout msec="10000">
          <!-- Grasp wrinkled_towel -->
          <Action ID="GRASP" obj="wrinkled_towel" />
        </Timeout>
      </RetryUntilSuccessful>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Unfold wrinkled_towel -->
      <Action ID="UNFOLD" obj="wrinkled_towel" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
