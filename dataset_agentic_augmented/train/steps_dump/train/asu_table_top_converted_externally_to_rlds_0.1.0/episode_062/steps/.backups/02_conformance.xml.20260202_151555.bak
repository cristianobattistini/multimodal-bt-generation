<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachBread -->
      <SubTree ID="ApproachBread" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachBread -->
  <BehaviorTree ID="ApproachBread">
    <Sequence>
      <!-- Navigate to bread -->
      <Action ID="NAVIGATE_TO" obj="bread" />
      <!-- Grasp bread -->
      <Action ID="GRASP" obj="bread" />
    </Sequence>
  </BehaviorTree>
</root>
