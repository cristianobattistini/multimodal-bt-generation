<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO blue_bottle within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to blue_bottle -->
        <Action ID="NAVIGATE_TO" obj="blue_bottle" />
      </Timeout>
      <!-- Grasp blue_bottle -->
      <Action ID="GRASP" obj="blue_bottle" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
    </Sequence>
  </BehaviorTree>
</root>
