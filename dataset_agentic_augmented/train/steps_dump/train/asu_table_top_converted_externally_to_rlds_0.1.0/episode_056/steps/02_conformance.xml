<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check red_milk_box is reachable before NAVIGATE_TO -->
      <Condition ID="IS_REACHABLE" obj="red_milk_box" />
      <!-- Timeout: NAVIGATE_TO red_milk_box within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to red_milk_box -->
        <Action ID="NAVIGATE_TO" obj="red_milk_box" />
      </Timeout>
      <!-- Push red_milk_box forward toward front edge of table -->
      <Action ID="PUSH" obj="red_milk_box" />
    </Sequence>
  </BehaviorTree>
</root>
