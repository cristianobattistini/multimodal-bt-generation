<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to white_carton -->
      <Action ID="NAVIGATE_TO" obj="white_carton" />
      <!-- Grasp white_carton -->
      <Action ID="GRASP" obj="white_carton" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Retry PLACE_ON_TOP table up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Place on table -->
        <Action ID="PLACE_ON_TOP" obj="table" />
      </RetryUntilSuccessful>
      
    </Sequence>
  </BehaviorTree>
</root>
