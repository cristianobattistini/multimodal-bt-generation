<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to red_bottle -->
      <Action ID="NAVIGATE_TO" obj="red_bottle" />
      <!-- Fallback: if GRASP red_bottle fails, try PUSH, FLIP to reposition -->
      <Fallback>
        <!-- Grasp red_bottle -->
        <Action ID="GRASP" obj="red_bottle" />
        <Sequence>
          <!-- Push red_bottle forward toward front edge of table -->
          <Action ID="PUSH" obj="red_bottle" />
          <!-- Grasp red_bottle -->
          <Action ID="GRASP" obj="red_bottle" />
        </Sequence>
      </Fallback>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
