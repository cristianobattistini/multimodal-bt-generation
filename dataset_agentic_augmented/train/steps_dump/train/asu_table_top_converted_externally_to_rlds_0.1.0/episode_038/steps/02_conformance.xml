<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check white_box is reachable before NAVIGATE_TO -->
      <Condition ID="IS_REACHABLE" obj="white_box" />
      <!-- Timeout: NAVIGATE_TO white_box within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to white_box -->
        <Action ID="NAVIGATE_TO" obj="white_box" />
      </Timeout>
      <!-- Grasp white_box -->
      <Action ID="GRASP" obj="white_box" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
    </Sequence>
  </BehaviorTree>
</root>
