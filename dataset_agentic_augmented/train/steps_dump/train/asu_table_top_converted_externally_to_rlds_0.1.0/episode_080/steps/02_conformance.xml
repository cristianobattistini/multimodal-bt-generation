<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO brown_object within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to brown_object -->
        <Action ID="NAVIGATE_TO" obj="brown_object" />
      </Timeout>
      <!-- Grasp brown_object -->
      <Action ID="GRASP" obj="brown_object" />
    </Sequence>
  </BehaviorTree>
</root>
