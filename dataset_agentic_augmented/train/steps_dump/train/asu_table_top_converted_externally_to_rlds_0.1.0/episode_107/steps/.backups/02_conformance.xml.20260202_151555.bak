<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to green_glass_bottle -->
      <Action ID="NAVIGATE_TO" obj="green_glass_bottle" />
      <Sequence>
        <!-- Check green_glass_bottle is reachable before GRASP -->
        <Condition ID="IS_REACHABLE" obj="green_glass_bottle" />
        <!-- Grasp green_glass_bottle -->
        <Action ID="GRASP" obj="green_glass_bottle" />
      </Sequence>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
