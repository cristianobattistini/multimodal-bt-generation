<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check red_can is visible before NAVIGATE_TO -->
      <Condition ID="IS_VISIBLE" obj="red_can" />
      <!-- Timeout: NAVIGATE_TO red_can within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to red_can -->
        <Action ID="NAVIGATE_TO" obj="red_can" />
      </Timeout>
      <!-- Grasp red_can -->
      <Action ID="GRASP" obj="red_can" />
    </Sequence>
  </BehaviorTree>
</root>
