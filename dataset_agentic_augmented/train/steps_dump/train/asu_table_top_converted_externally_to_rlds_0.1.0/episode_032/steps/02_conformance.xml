<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachRedBottle -->
      <SubTree ID="ApproachRedBottle" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachRedBottle -->
  <BehaviorTree ID="ApproachRedBottle">
    <Sequence>
      <!-- Navigate to red_bottle -->
      <Action ID="NAVIGATE_TO" obj="red_bottle" />
      <!-- Grasp red_bottle -->
      <Action ID="GRASP" obj="red_bottle" />
    </Sequence>
  </BehaviorTree>
</root>
