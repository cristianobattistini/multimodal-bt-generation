<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <Sequence>
        <!-- Check lid is reachable before NAVIGATE_TO -->
        <Condition ID="IS_REACHABLE" obj="lid" />
        <!-- Navigate to lid -->
        <Action ID="NAVIGATE_TO" obj="lid" />
      </Sequence>
      <!-- Open lid -->
      <Action ID="OPEN" obj="lid" />
    </Sequence>
  </BehaviorTree>
</root>
