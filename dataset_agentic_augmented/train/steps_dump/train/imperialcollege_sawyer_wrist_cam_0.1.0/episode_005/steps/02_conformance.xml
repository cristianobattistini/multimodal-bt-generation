<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO lid within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to lid -->
        <Action ID="NAVIGATE_TO" obj="lid" />
      </Timeout>
      <!-- Open lid -->
      <Action ID="OPEN" obj="lid" />
    </Sequence>
  </BehaviorTree>
</root>
