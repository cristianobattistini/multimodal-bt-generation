<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachPan -->
      <SubTree ID="ApproachPan" />
      <!-- Return pan upright (flip) -->
      <Action ID="FLIP" obj="pan" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachPan -->
  <BehaviorTree ID="ApproachPan">
    <Sequence>
      <!-- Navigate to pan -->
      <Action ID="NAVIGATE_TO" obj="pan" />
      <!-- Grasp pan -->
      <Action ID="GRASP" obj="pan" />
    </Sequence>
  </BehaviorTree>
</root>
