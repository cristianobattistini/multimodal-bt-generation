<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to apple -->
      <Action ID="NAVIGATE_TO" obj="apple" />
      <!-- Grasp apple -->
      <Action ID="GRASP" obj="apple" />
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Retry PLACE_INSIDE pot up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Place inside pot -->
        <Action ID="PLACE_INSIDE" obj="pot" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
