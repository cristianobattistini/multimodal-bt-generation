<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_gear -->
      <Action ID="NAVIGATE_TO" obj="blue_gear" />
      <!-- Grasp blue_gear -->
      <Action ID="GRASP" obj="blue_gear" />
      <!-- Navigate to right_peg -->
      <Action ID="NAVIGATE_TO" obj="right_peg" />
      <!-- Place on right_peg -->
      <Action ID="PLACE_ON_TOP" obj="right_peg" />
      <!-- Release -->
      <Action ID="RELEASE" />
      <!-- Navigate to red_gear -->
      <Action ID="NAVIGATE_TO" obj="red_gear" />
      <!-- Check red_gear is reachable before GRASP -->
      <Condition ID="IS_REACHABLE" obj="red_gear" />
      <!-- Retry GRASP red_gear up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Grasp red_gear -->
        <Action ID="GRASP" obj="red_gear" />
      </RetryUntilSuccessful>
      <!-- Navigate to right_peg -->
      <Action ID="NAVIGATE_TO" obj="right_peg" />
      <!-- Place on right_peg -->
      <Action ID="PLACE_ON_TOP" obj="right_peg" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
