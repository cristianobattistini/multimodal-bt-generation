<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_gear -->
      <Action ID="NAVIGATE_TO" obj="blue_gear" />
      <!-- Grasp blue_gear -->
      <Action ID="GRASP" obj="blue_gear" />
      <!-- Navigate to right_peg -->
      <Action ID="NAVIGATE_TO" obj="right_peg" />
      <!-- Fallback: if PLACE_ON_TOP right_peg fails, try PUSH to reposition -->
      <Fallback>
        <!-- Place on right_peg -->
        <Action ID="PLACE_ON_TOP" obj="right_peg" />
        <Sequence>
          <!-- Push right_peg forward toward front edge of table -->
          <Action ID="PUSH" obj="right_peg" />
          <!-- Place on right_peg -->
          <Action ID="PLACE_ON_TOP" obj="right_peg" />
        </Sequence>
      </Fallback>
      <!-- Release -->
      <Action ID="RELEASE" />
      <!-- Navigate to red_gear -->
      <Action ID="NAVIGATE_TO" obj="red_gear" />
      <!-- Grasp red_gear -->
      <Action ID="GRASP" obj="red_gear" />
      <!-- Navigate to right_peg -->
      <Action ID="NAVIGATE_TO" obj="right_peg" />
      <!-- Fallback: if PLACE_ON_TOP right_peg fails, try PUSH to reposition -->
      <Fallback>
        <!-- Place on right_peg -->
        <Action ID="PLACE_ON_TOP" obj="right_peg" />
        <Sequence>
          <!-- Push right_peg forward toward front edge of table -->
          <Action ID="PUSH" obj="right_peg" />
          <!-- Place on right_peg -->
          <Action ID="PLACE_ON_TOP" obj="right_peg" />
        </Sequence>
      </Fallback>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
