<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachFridgeDoor -->
      <SubTree ID="ApproachFridgeDoor" />
      <!-- Open fridge_door -->
      <Action ID="OPEN" obj="fridge_door" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachFridgeDoor -->
  <BehaviorTree ID="ApproachFridgeDoor">
    <Sequence>
      <!-- Navigate to fridge_door -->
      <Action ID="NAVIGATE_TO" obj="fridge_door" />
      <!-- Grasp fridge_door -->
      <Action ID="GRASP" obj="fridge_door" />
    </Sequence>
  </BehaviorTree>
</root>
