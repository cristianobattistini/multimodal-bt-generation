<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pitcher -->
      <Action ID="NAVIGATE_TO" obj="pitcher" />
      <!-- Grasp pitcher -->
      <Action ID="GRASP" obj="pitcher" />
      <!-- Navigate to mug -->
      <Action ID="NAVIGATE_TO" obj="mug" />
      <!-- Pour into mug -->
      <Action ID="POUR" obj="mug" />
      <!-- Timeout: NAVIGATE_TO table within 10s -->
      <Timeout msec="10000">
        <!-- Navigate to table -->
        <Action ID="NAVIGATE_TO" obj="table" />
      </Timeout>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
