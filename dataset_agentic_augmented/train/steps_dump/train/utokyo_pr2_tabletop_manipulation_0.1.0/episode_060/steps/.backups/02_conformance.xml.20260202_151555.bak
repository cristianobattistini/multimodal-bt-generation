<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to bread -->
      <Action ID="NAVIGATE_TO" obj="bread" />
      <!-- Grasp bread -->
      <Action ID="GRASP" obj="bread" />
      <!-- Navigate to white_plate -->
      <Action ID="NAVIGATE_TO" obj="white_plate" />
      <!-- Retry PLACE_ON_TOP white_plate up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Place on white_plate -->
        <Action ID="PLACE_ON_TOP" obj="white_plate" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
