<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO bread within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to bread -->
        <Action ID="NAVIGATE_TO" obj="bread" />
      </Timeout>
      <!-- Grasp bread -->
      <Action ID="GRASP" obj="bread" />
    </Sequence>
  </BehaviorTree>
</root>
