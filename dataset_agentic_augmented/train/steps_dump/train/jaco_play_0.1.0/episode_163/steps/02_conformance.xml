<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to black_bowl -->
      <Action ID="NAVIGATE_TO" obj="black_bowl" />
      <!-- Retry GRASP black_bowl up to 4 attempts -->
      <RetryUntilSuccessful num_attempts="4">
        <!-- Grasp black_bowl -->
        <Action ID="GRASP" obj="black_bowl" />
      </RetryUntilSuccessful>
      <!-- Navigate to oven -->
      <Action ID="NAVIGATE_TO" obj="oven" />
      <!-- Place inside oven -->
      <Action ID="PLACE_INSIDE" obj="oven" />
    </Sequence>
  </BehaviorTree>
</root>
