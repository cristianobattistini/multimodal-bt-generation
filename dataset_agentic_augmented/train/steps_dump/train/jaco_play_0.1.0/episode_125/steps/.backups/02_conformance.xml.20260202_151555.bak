<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to black_bowl -->
      <Action ID="NAVIGATE_TO" obj="black_bowl" />
      <!-- Grasp black_bowl -->
      <Action ID="GRASP" obj="black_bowl" />
      <!-- Navigate to sink -->
      <Action ID="NAVIGATE_TO" obj="sink" />
      <!-- Retry PLACE_INSIDE sink up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Place inside sink -->
        <Action ID="PLACE_INSIDE" obj="sink" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
