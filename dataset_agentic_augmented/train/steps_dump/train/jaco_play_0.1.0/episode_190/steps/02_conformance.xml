<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Timeout: NAVIGATE_TO long_bread within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to long_bread -->
        <Action ID="NAVIGATE_TO" obj="long_bread" />
      </Timeout>
      <!-- Grasp long_bread -->
      <Action ID="GRASP" obj="long_bread" />
      <!-- Navigate to sink -->
      <Action ID="NAVIGATE_TO" obj="sink" />
      <!-- Place inside sink -->
      <Action ID="PLACE_INSIDE" obj="sink" />
    </Sequence>
  </BehaviorTree>
</root>
