<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachGrayBowl -->
      <SubTree ID="ApproachGrayBowl" />
      <!-- Navigate to oven -->
      <Action ID="NAVIGATE_TO" obj="oven" />
      <!-- Place inside oven -->
      <Action ID="PLACE_INSIDE" obj="oven" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachGrayBowl -->
  <BehaviorTree ID="ApproachGrayBowl">
    <Sequence>
      <!-- Navigate to gray_bowl -->
      <Action ID="NAVIGATE_TO" obj="gray_bowl" />
      <!-- Grasp gray_bowl -->
      <Action ID="GRASP" obj="gray_bowl" />
    </Sequence>
  </BehaviorTree>
</root>
