<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check table is reachable before NAVIGATE_TO -->
      <Condition ID="IS_REACHABLE" obj="table" />
      <!-- Timeout: NAVIGATE_TO table within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to blue -->
        <Action ID="NAVIGATE_TO" obj="blue" />
      </Timeout>
      <!-- Place next to blue -->
      <Action ID="PLACE_NEXT_TO" obj="blue" />
    </Sequence>
  </BehaviorTree>
</root>
