<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to butter_dairy -->
      <Action ID="NAVIGATE_TO" obj="butter_dairy" />
      <!-- Grasp butter_dairy -->
      <Action ID="GRASP" obj="butter_dairy" />
      <!-- Navigate to gray_bowl -->
      <Action ID="NAVIGATE_TO" obj="gray_bowl" />
      <!-- Check gray_bowl is open before PLACE_INSIDE -->
      <Condition ID="IS_OPEN" obj="gray_bowl" />
      <!-- Retry PLACE_INSIDE gray_bowl up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Place inside gray_bowl -->
        <Action ID="PLACE_INSIDE" obj="gray_bowl" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
