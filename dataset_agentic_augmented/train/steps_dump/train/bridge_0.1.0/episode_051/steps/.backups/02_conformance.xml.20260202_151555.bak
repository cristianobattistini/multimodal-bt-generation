<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pan -->
      <Action ID="NAVIGATE_TO" obj="pan" />
      <Sequence>
        <!-- Check pan is reachable before GRASP -->
        <Condition ID="IS_REACHABLE" obj="pan" />
        <!-- Grasp pan -->
        <Action ID="GRASP" obj="pan" />
      </Sequence>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
