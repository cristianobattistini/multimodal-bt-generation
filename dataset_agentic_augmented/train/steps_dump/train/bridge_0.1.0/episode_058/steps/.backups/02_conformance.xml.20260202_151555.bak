<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to pot -->
      <Action ID="NAVIGATE_TO" obj="pot" />
      <!-- Grasp pot -->
      <Action ID="GRASP" obj="pot" />
      <!-- Timeout: NAVIGATE_TO table within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to table -->
        <Action ID="NAVIGATE_TO" obj="table" />
      </Timeout>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
