      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachStrawberry -->
  <BehaviorTree ID="ApproachStrawberry">
    <Sequence>
      <!-- Navigate to strawberry -->
      <Action ID="NAVIGATE_TO" obj="strawberry" />
      <!-- Grasp strawberry -->
      <Action ID="GRASP" obj="strawberry" />
    </Sequence>
  </BehaviorTree>
</root>
