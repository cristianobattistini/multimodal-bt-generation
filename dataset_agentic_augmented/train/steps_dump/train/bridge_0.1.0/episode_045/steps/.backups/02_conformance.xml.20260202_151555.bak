<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to potato -->
      <Action ID="NAVIGATE_TO" obj="potato" />
      <!-- Grasp potato -->
      <Action ID="GRASP" obj="potato" />
      <!-- Navigate to pot_cardboard_fence -->
      <Action ID="NAVIGATE_TO" obj="pot_cardboard_fence" />
      <!-- Retry PLACE_INSIDE pot_cardboard_fence up to 3 attempts -->
      <RetryUntilSuccessful num_attempts="3">
        <!-- Place inside pot_cardboard_fence -->
        <Action ID="PLACE_INSIDE" obj="pot_cardboard_fence" />
      </RetryUntilSuccessful>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
