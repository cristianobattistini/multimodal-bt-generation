<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to silver_pot -->
      <Action ID="NAVIGATE_TO" obj="silver_pot" />
      <!-- Grasp silver_pot -->
      <Action ID="GRASP" obj="silver_pot" />
      <!-- Timeout: NAVIGATE_TO blue_towel within 20s -->
      <Timeout msec="20000">
        <!-- Navigate to blue_towel -->
        <Action ID="NAVIGATE_TO" obj="blue_towel" />
      </Timeout>
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
