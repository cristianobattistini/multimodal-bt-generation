<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to vessel -->
      <Action ID="NAVIGATE_TO" obj="vessel" />
      <!-- Grasp vessel -->
      <Action ID="GRASP" obj="vessel" />
      <!-- Navigate to violet_cloth -->
      <Action ID="NAVIGATE_TO" obj="violet_cloth" />
      <Sequence>
        <!-- Check violet_cloth is holding before PLACE_ON_TOP -->
        <Condition ID="IS_HOLDING" obj="violet_cloth" />
        <!-- Place on violet_cloth -->
        <Action ID="PLACE_ON_TOP" obj="violet_cloth" />
      </Sequence>
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
