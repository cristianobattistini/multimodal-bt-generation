<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachMetalBowl -->
      <SubTree ID="ApproachMetalBowl" />
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachMetalBowl -->
  <BehaviorTree ID="ApproachMetalBowl">
    <Sequence>
      <!-- Navigate to metal_bowl -->
      <Action ID="NAVIGATE_TO" obj="metal_bowl" />
      <!-- Grasp metal_bowl -->
      <Action ID="GRASP" obj="metal_bowl" />
    </Sequence>
  </BehaviorTree>
</root>
