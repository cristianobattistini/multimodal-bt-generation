<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to kadai -->
      <Action ID="NAVIGATE_TO" obj="kadai" />
      <Sequence>
        <!-- Check kadai is visible before GRASP -->
        <Condition ID="IS_VISIBLE" obj="kadai" />
        <!-- Grasp kadai -->
        <Action ID="GRASP" obj="kadai" />
      </Sequence>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
