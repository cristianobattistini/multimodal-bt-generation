<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachSpoon -->
      <SubTree ID="ApproachSpoon" />
      <!-- Navigate to vessel -->
      <Action ID="NAVIGATE_TO" obj="vessel" />
      <!-- Place next to vessel -->
      <Action ID="PLACE_NEXT_TO" obj="vessel" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachSpoon -->
  <BehaviorTree ID="ApproachSpoon">
    <Sequence>
      <!-- Navigate to spoon -->
      <Action ID="NAVIGATE_TO" obj="spoon" />
      <!-- Grasp spoon -->
      <Action ID="GRASP" obj="spoon" />
    </Sequence>
  </BehaviorTree>
</root>
