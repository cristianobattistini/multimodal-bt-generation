<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachCorn -->
      <SubTree ID="ApproachCorn" />
      <!-- Navigate to bowl -->
      <Action ID="NAVIGATE_TO" obj="bowl" />
      <!-- Place inside bowl -->
      <Action ID="PLACE_INSIDE" obj="bowl" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachCorn -->
  <BehaviorTree ID="ApproachCorn">
    <Sequence>
      <!-- Navigate to corn -->
      <Action ID="NAVIGATE_TO" obj="corn" />
      <!-- Grasp corn -->
      <Action ID="GRASP" obj="corn" />
    </Sequence>
  </BehaviorTree>
</root>
