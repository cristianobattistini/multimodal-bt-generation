<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to metal_bowl -->
      <Action ID="NAVIGATE_TO" obj="metal_bowl" />
      <!-- Fallback: if GRASP cheese_wedge fails, try alternative to reposition -->
      <Fallback>
        <!-- Retry GRASP cheese_wedge up to 3 attempts -->
        <RetryUntilSuccessful num_attempts="3">
          <!-- Grasp cheese_wedge -->
          <Action ID="GRASP" obj="cheese_wedge" />
        </RetryUntilSuccessful>
        <Sequence>
          <!-- Push cheese_wedge forward toward front edge of table -->
          <Action ID="PUSH" obj="cheese_wedge" />
          <!-- Grasp cheese_wedge -->
          <Action ID="GRASP" obj="cheese_wedge" />
        </Sequence>
      </Fallback>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
