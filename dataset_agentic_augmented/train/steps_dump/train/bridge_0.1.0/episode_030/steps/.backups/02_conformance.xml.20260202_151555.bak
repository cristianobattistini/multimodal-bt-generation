<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Navigate to blue_fork -->
      <Action ID="NAVIGATE_TO" obj="blue_fork" />
      <!-- Retry GRASP blue_fork up to 5 attempts -->
      <RetryUntilSuccessful num_attempts="5">
        <!-- Grasp blue_fork -->
        <Action ID="GRASP" obj="blue_fork" />
      </RetryUntilSuccessful>
      <!-- Navigate to table -->
      <Action ID="NAVIGATE_TO" obj="table" />
      <!-- Place on table -->
      <Action ID="PLACE_ON_TOP" obj="table" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
