<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachBowl -->
      <SubTree ID="ApproachBowl" />
      <!-- Navigate to yellow_cloth -->
      <Action ID="NAVIGATE_TO" obj="yellow_cloth" />
      <!-- Place on yellow_cloth -->
      <Action ID="PLACE_ON_TOP" obj="yellow_cloth" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachBowl -->
  <BehaviorTree ID="ApproachBowl">
    <Sequence>
      <!-- Navigate to bowl -->
      <Action ID="NAVIGATE_TO" obj="bowl" />
      <!-- Grasp bowl -->
      <Action ID="GRASP" obj="bowl" />
    </Sequence>
  </BehaviorTree>
</root>
