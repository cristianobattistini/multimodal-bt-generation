<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Execute subtree ApproachCannedSpam -->
      <SubTree ID="ApproachCannedSpam" />
      <!-- Navigate to sink -->
      <Action ID="NAVIGATE_TO" obj="sink" />
      <!-- Place inside sink -->
      <Action ID="PLACE_INSIDE" obj="sink" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
  <!-- Subtree definition ApproachCannedSpam -->
  <BehaviorTree ID="ApproachCannedSpam">
    <Sequence>
      <!-- Navigate to canned_spam -->
      <Action ID="NAVIGATE_TO" obj="canned_spam" />
      <!-- Retry GRASP canned_spam up to 2 attempts -->
      <RetryUntilSuccessful num_attempts="2">
        <!-- Grasp canned_spam -->
        <Action ID="GRASP" obj="canned_spam" />
      </RetryUntilSuccessful>
    </Sequence>
  </BehaviorTree>
</root>
