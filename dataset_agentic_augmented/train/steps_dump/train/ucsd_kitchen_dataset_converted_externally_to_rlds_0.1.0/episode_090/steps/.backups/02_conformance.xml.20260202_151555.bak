<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence>
      <!-- Check teapot is visible before NAVIGATE_TO -->
      <Condition ID="IS_VISIBLE" obj="teapot" />
      <!-- Timeout: NAVIGATE_TO teapot within 15s -->
      <Timeout msec="15000">
        <!-- Navigate to teapot -->
        <Action ID="NAVIGATE_TO" obj="teapot" />
      </Timeout>
      <!-- Grasp teapot -->
      <Action ID="GRASP" obj="teapot" />
      <!-- Navigate to stove -->
      <Action ID="NAVIGATE_TO" obj="stove" />
      <!-- Place on stove -->
      <Action ID="PLACE_ON_TOP" obj="stove" />
      <!-- Release -->
      <Action ID="RELEASE" />
    </Sequence>
  </BehaviorTree>
</root>
